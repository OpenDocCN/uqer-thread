  #print stock\n        beat_me(account,stock)\n\n\ndef beat_me(account,stock):\n    global top_price\n    sale_date = account.current_date\n    data = DataFrame()\n    global total_index\n    global zt_state\n    global zt_ma5_price\n    snapshot_data=DataAPI.MktStockFactorsOneDayGet(tradeDate=sale_date,secID=stock,field=fields,pandas=\"1\")\n    diff=snapshot_data.get(\"DIFF\").get(0)\n    dea=snapshot_data.get(\"DEA\").get(0)\n    ma5=snapshot_data.get(\"MA5\").get(0)\n    ma10=snapshot_data.get(\"MA10\").get(0)\n    \n    \n    if account.security_position.has_key(stock):\n        if account.reference_return[stock]<diefu:\n            order_to(stock, 0)\n        \n    \n    if not zt_state.has_key(stock) :\n        zt_state[stock]=1\n        \n        \n    if diff>dea : \n        if zt_state[stock]==0 :\n            order_pct(stock, buy_precent)\n        zt_state[stock]=1\n    else :\n        if zt_state[stock]==1 :\n            #sell\n            order_to(stock, 0)\n        zt_state[stock]=0\n    return\n\n\ndef shizixing():\n    data=history(attribute='closePrice', symbol='all', time_range=1, freq='d', **options)\n    print data\n\n@staticmethod\ndef getYesterday():\n    itoday=dt.today()  \n    ioneday=timedelta(days=1)      \n    itoday=itoday-ioneday  \n    return itoday\n\n    \n    \ndef compare(list1,list2,data):\n    a=0\n    idata={}\n    list_tradeDate=[]\n    list_MA5=[]\n    list_MACD=[]\n    for index,value in enumerate(list1):\n        if list1.get(index)>list2.get(index) and  list1.get(index-1)<list2.get(index-1) :  #\u91d1\u53c9\n            list_tradeDate.append(data['tradeDate'].get(index))\n            list_MA5.append(data['MA5'].get(index))\n            list_MACD.append(data['MACD'].get(index))\n    \n    idata['tradeDate']=list_tradeDate\n    idata['MA5']=list_MA5\n    idata['MACD']=list_MACD\n    isPriceRight(idata)\n    return idata\n\ndef isPriceRight(idata):\n    list1=idata['MA5']\n    #print idata\n    list_ok=[]\n    \n    for index,value in enumerate(list1):\n        if list1[index]>list1[index-1] :\n            list_ok.append('10')\n            \n            #order_pct(mysecID, 0.5)\n        elif list1[index]*1.01>list1[index-1] :\n            list_ok.append('8')\n            #order_pct(mysecID, 0.2)\n        else:\n            list_ok.append('0')\n            \n    idata['list_ok']=list_ok\n    print list_ok\n\n\n#get_data(0).head(50)\n\n# while 1: \n#     this_data=get_data().head(50)\n#     print(this_data)\n#     print(\"\\r\\n\\r\\n\\r\\n\\r\\n\\r\\n\\r\\n\\r\\n\\r\\n\")\n#     time.sleep(5)\n", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "2017-03-22"}, {"output_type": "stream", "stream": "stdout", "text": "\n"}, {"output_type": "stream", "stream": "stdout", "text": "SYSTEM ERROR: Fail to save backtest result.\n"}, {"metadata": {}, "output_type": "pyout", "prompt_number": 2, "text": "'{\"information\": -0.72832, \"benchmark_cumulative_return\": {\"1487030400000\": 0.03798185, \"1488499200000\": 0.035582815, \"1483920000000\": 0.0162594208, \"1485129600000\": 0.0163138002, \"1486080000000\": 0.0164379663, \"1489536000000\": 0.0463925203, \"1488412800000\": 0.0377679579, \"1486944000000\": 0.0381241426, \"1487808000000\": 0.04931692, \"1483574400000\": 0.0174340145, \"1483488000000\": 0.0175920166, \"1484179200000\": 0.0022787962, \"1484611200000\": 0.0049167981, \"1488153600000\": 0.0411295071, \"1489708800000\": 0.0411246734, \"1489449600000\": 0.0442925717, \"1487548800000\": 0.0487335506, \"1485302400000\": 0.0198856161, \"1489622400000\": 0.051789065, \"1488844800000\": 0.0434660058, \"1483401600000\": 0.0097115448, \"1486684800000\": 0.0312397189, \"1487635200000\": 0.0521866383, \"1488240000000\": 0.0431194886, \"1488758400000\": 0.0412083571, \"1487721600000\": 0.0542814511, \"1485216000000\": 0.0164255799, \"1490054400000\": 0.0471104554, \"1486425600000\": 0.0167983805, \"1487203200000\": 0.0395313589, \"1486598400000\"