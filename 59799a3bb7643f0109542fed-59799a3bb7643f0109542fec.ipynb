{"metadata": {"signature": "sha256:89238e36c83c76c66f273f91d672e88fb834ee391e72402196048ed3bb799278"}, "nbformat": 3, "nbformat_minor": 0, "original_thread": "55ff6ce9f9f06c597265ef04", "worksheets": [{"cells": [{"cell_type": "markdown", "id": "BF7513A8901F4D188AA95F07A27ACED5", "metadata": {}, "source": "# \uff08\u63d0\u95ee\uff09\u91cf\u5316\u9009\u80a12"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "C918EF8B56C642D5B89E7F9F1963BBC2", "input": "# \u5bfc\u5165\u5305\nfrom CAL.PyCAL import *\nimport numpy as np\nimport pandas as pd\n\n# \u6784\u5efa\u65e5\u671f\u5217\u8868\uff0c\u4ee5\u4fdd\u8bc1\u6bcf\u6708\u7b2c\u4e00\u4e2a\u4ea4\u6613\u65e5\u5efa\u4ed3\ndata=DataAPI.TradeCalGet(exchangeCD=u\"XSHG\",beginDate=u\"20070101\",field=['calendarDate','isWeekEnd','isMonthEnd'],pandas=\"1\")\ndata = data[data['isWeekEnd'] == 1]\ndate_list = map(lambda x: x[0:4]+x[5:7]+x[8:10], data['calendarDate'].values.tolist())\n\nstart = '2017-01-01'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2017-07-26'                        # \u56de\u6d4b\u7ed3\u675f\u65f6\u95f4\nuniverse = set_universe('HS300') + set_universe('ZZ500')   # \u80a1\u7968\u6c60\nbenchmark = 'HS300'                       # \u7b56\u7565\u53c2\u8003\u6807\u51c6\ncapital_base = 10000000                     # \u8d77\u59cb\u8d44\u91d1\nfreq = 'd'                              # \u7b56\u7565\u7c7b\u578b\uff0c'd'\u8868\u793a\u65e5\u95f4\u7b56\u7565\u4f7f\u7528\u65e5\u7ebf\u56de\u6d4b\uff0c'm'\u8868\u793a\u65e5\u5185\u7b56\u7565\u4f7f\u7528\u5206\u949f\u7ebf\u56de\u6d4b\nrefresh_rate = 1                          # \u8c03\u4ed3\u9891\u7387\ncommission = Commission(buycost=0, sellcost=0)\n\n# \u65e5\u671f\u5904\u7406\u76f8\u5173\ncal = Calendar('China.SSE')\nperiod = Period('-1B')\nfactor_name = ['PE','FY12P']\n\ndef initialize(account):                   # \u521d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    pass\n\ndef handle_data(account):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    \n    today = account.current_date\n    today = Date.fromDateTime(account.current_date)  # \u5411\u524d\u79fb\u52a8\u4e00\u4e2a\u5de5\u4f5c\u65e5\n    yesterday = cal.advanceDate(today, period)\n    yesterday = yesterday.toDateTime().strftime('%Y%m%d')\n    if yesterday in date_list:\n        \n        Factor1 = DataAPI.MktStockFactorsOneDayGet(tradeDate=yesterday,secID=account.universe[:400],field=['secID','PE','FY12P'],pandas=\"1\")\n        Factor2 = DataAPI.MktStockFactorsOneDayGet(tradeDate=yesterday,secID=account.universe[400:],field=['secID','PE','FY12P'],pandas=\"1\")\n        Factor = pd.concat([Factor1, Factor2])\n        Factor['ticker'] = Factor['secID'].apply(lambda x: x[0:6])\n        Factor.set_index('ticker',inplace=True)\n        \n        # \n        Factor['PE'] = 1.0 / Factor['PE']  #\n        factor = Factor['PE'].dropna().to_dict()\n        signal_PE = standardize(neutralize(winsorize(factor),yesterday))   # \u56e0\u5b50\u5904\u7406\n        \n        # \n        Factor['FY12P'] = Factor['FY12P']  # \n        factor = Factor['FY12P'].dropna().to_dict()\n        signal_FY12P = standardize(neutralize(winsorize(factor),yesterday))  # \u56e0\u5b50\u5904\u7406\n        \n        # \u6784\u5efa\u7ec4\u5408score\u77e9\u9635\n        Total_Score = pd.DataFrame(index=Factor.index, columns=factor_name, data=0)\n        Total_Score['PE'][signal_PE.keys()] = signal_PE.values()\n        Total_Score['FY12P'][signal_FY12P.keys()] = signal_FY12P.values()\n        # Total_Score['FinancialExpenseRate'][signal_FinancialExpenseRate.keys()]= signal_FinancialExpenseRate.values()\n        Total_Score['total_score'] = np.dot(Total_Score, np.array([1,1]))  # \u7efc\u5408\u4e24\u4e2a\u56e0\u5b50\u7684\u5927\u5c0f\uff0c\u4e0d\u5931\u4e00\u822c\u6027\uff0c\u7b49\u6743\u6c42\u548c\n        total_score = Total_Score['total_score'].to_dict()\n        \n        wts = long_only(total_score, 0, 0.0067, 3, yesterday)  \n        log.info(wts)\n        \n        Factor['wts'] = np.nan\n        Factor['wts'][wts.keys()] = wts.values()\n        Factor = Factor[~np.isnan(Factor['wts'])]\n        Factor.set_index('secID', inplace=True)\n        Factor.drop(factor_name, axis=1, inplace=True)\n                \n        # \u4ea4\u6613\u90e8\u5206\n        sell_list = [stk for stk in account.security_position if stk not in wts]\n        for stk in sell_list:\n            order_to(stk,0)\n\n    c = account.reference_portfolio_value\n    change = {}\n    for stock, w in wts.iteritems():\n        p = account.reference_price[stock]\n        if not np.isnan(p) and p > 0:\n            change[stock] = int(c * w / p) - account.security_position.get(stock, 0)\n\n    for stock in sorted(change, key=change.get):\n        order(stock, change[stock])\n    \n    else:\n        return", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "2017-01-03 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] {'600177': 0.12185760279735108, '000600': 0.14105997284696567, '000625': 0.11661701383412883, '600104': 0.11881474925981013, '002352': 0.20901160829285093, '600100': 0.2926390529688933}\n"}, {"ename": "KeyError", "evalue": "'600177'", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mKeyError\u001b[0m                                  Traceback (most recent call last)", "\u001b[1;32m<mercury-input-13-C918EF8B56C642D5B89E7F9F1963BBC2>\u001b[0m in \u001b[0;36m<module>\u001b[1;34m()\u001b[0m\n\u001b[0;32m    123\u001b[0m                                             \u001b[0msecurity_cost\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0msecurity_cost\u001b[0m\u001b[1;33m,\u001b[0m \u001b[0mmax_history_window\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0mmax_history_window\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\u001b[0m\u001b[0m\n\u001b[0;32m    1