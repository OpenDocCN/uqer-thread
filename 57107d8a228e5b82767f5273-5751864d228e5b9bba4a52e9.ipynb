[0]) or opn.get(s, 0)[0] == 0)] \n    STlist = DataAPI.SecSTGet(secID=account.universe, beginDate=yesterday, endDate=yesterday, field=['secID'])\n    valid_universe2 = [s for s in account.universe if s not in STlist]\n    valid_universe=list(set(valid_universe1+valid_universe2))\n\n    history = account.get_attribute_history('closePrice', 10)\n    momentum = {'secID':[], 'c_ret':[]}\n    for s in valid_universe:\n        momentum['secID'].append(s)\n        momentum['c_ret'].append(history[s][-1]/history[s][0])\n        \n    momentum = pd.DataFrame(momentum).sort(columns='c_ret',ascending=[True]).reset_index()\n    momentum = momentum[len(momentum)*8/12:len(momentum)]   \n    \n    contrarian = {'secID':[], 'c_ret':[]}\n    for s in valid_universe:\n        contrarian['secID'].append(s)\n        contrarian['c_ret'].append(history[s][-1]/history[s][0])\n        \n    contrarian = pd.DataFrame(contrarian).sort(columns='c_ret',ascending=[False]).reset_index()\n    contrarian = contrarian[len(contrarian)*8/12:len(contrarian)]   \n\n    portfolio=momentum.append(contrarian)\n            \n            \n    choicenum = 8\n    open_price = account.get_attribute_history('openPrice', 1)    # \u524d\u4e00\u65e5\u5f00\u76d8\u4ef7\n    close_price = account.referencePrice                   # \u524d\u4e00\u65e5\u6536\u76d8\u4ef7\n\n    \n    \n    last_day = cal.advanceDate(date,'-1B',BizDayConvention.Preceding).strftime(\"%Y%m%d\")            #\u8ba1\u7b97\u51fa\u5012\u6570\u7b2c\u4e00\u4e2a\u4ea4\u6613\u65e5\n    mkt_value = DataAPI.MktEqudGet(secID=portfolio['secID'],tradeDate=last_day,field=\"secID,negMarketValue\",pandas=\"1\")\n    s_list = mkt_value.sort('negMarketValue').secID.head(choicenum).tolist()\n    #.sort('negMarketValue').secID.head(choicenum).tolist()\n    buylist = {s: 0 for s in s_list}\n   \n\n    for s in account.valid_secpos:\n        if s not in buylist:\n            order_to(s, 0)\n            \n    v = account.referencePortfolioValue / len(buylist) \n 