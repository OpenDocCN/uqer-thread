{"metadata": {"signature": "sha256:da491cecd2230f933a2f98c79e5858857c473054363f652038def891f5c57de4"}, "nbformat": 3, "nbformat_minor": 0, "original_thread": "57600fcd228e5b8a38285d48", "worksheets": [{"cells": [{"cell_type": "markdown", "id": "5C7B2CA9961D49279F7CC7DEC9E4E9B6", "metadata": {}, "source": "# \uff3b\u4ece\u96f6\u5f00\u59cb\u5b66uqer\uff3dMACD\u4ea4\u6613\u7b56\u7565"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "5159752348C5468F99549682E5DD02A0", "input": "from CAL.PyCAL import Date\nfrom CAL.PyCAL import Calendar\nfrom CAL.PyCAL import BizDayConvention\n\nstart = '2013-01-01'\nend = u''\nbenchmark = '000001.ZICN'\nuniverse = set_universe('A')\ncapital_base = 100000\nrefresh_rate = 1\ncommission = Commission(buycost=0.0003, sellcost=0.002, unit='perValue')\n\ndef initialize(account):\n    #account.universe = universe\n    pass\n\ndef handle_data(account):\n    buylist=[] #\u4e70\u5355\n    selist=[] #\u5356\u5355\n    dt = Date.fromDateTime(account.current_date)  #\u4ecepython\u6807\u51c6\u5e93datetime\u7c7b\u578b\u8f6c\u5316\u4e3acal\u7684date\u7c7b\u578b\n    cal = Calendar('China.SSE') #\u4e0a\u6d77\u8bc1\u5238\u4ea4\u6613\u6240\u4ea4\u6613\u65e5\u5386\n    lastTDay = cal.advanceDate(dt,'-1B',BizDayConvention.Preceding) #\u5728cal\u65e5\u5386\u4e0b\u5f53\u524d\u65e5\u671f\u7684\u524d\u4e00\u5de5\u4f5c\u65e5\n    current_date=dt.strftime('%Y%m%d') #\u8f6c\u6362\u5b57\u7b26\u4e32\u6a21\u5f0f\uff0c\u9002\u7528\u4e8ecal\u7684date\n    last_date=lastTDay.strftime('%Y%m%d')\n    \n    getData_current_date=DataAPI.MktStockFactorsOneDayGet(tradeDate=current_date,secID=account.universe,field=['secID','MACD','DEA','DIFF','NetProfitGrowRate'],pandas=\"1\")#\u83b7\u53d6\u5f53\u65e5\u80a1\u7968\u6c60\u4e2d\u7684MACD\uff0fDEA\uff0fDIFF\uff0f\u51c0\u5229\u6da6\u589e\u957f\u7387\uff08Net profit growth rate\uff09\u3002\u8ba1\u7b97\u65b9\u6cd5\uff1a\u51c0\u5229\u6da6\u589e\u957f\u7387=(\u4eca\u5e74\u51c0\u5229\u6da6\uff08TTM\uff09/\u53bb\u5e74\u51c0\u5229\u6da6\uff08TTM\uff09)-1\n    getData_current_date.set_index('secID',inplace=True) #\u52a0\u5165\u4e00\u5217\u7d22\u5f15secID\n    getData_current_date=getData_current_date[getData_current_date.NetProfitGrowRate>=1.0].dropna() #\u9009\u53d6\u51c0\u5229\u6da6\u589e\u957f\u7387\u5927\u4e8e\u7b49\u4e8e1\u7684\u80a1\u7968\u5e76\u5220\u9664na\u9879\n    getData_current_date=getData_current_date.sort(columns='NetProfitGrowRate',ascending=False)#\u6839\u636e\u51c0\u5229\u6da6\u589e\u957f\u7387\u964d\u5e8f\u6392\u5217\n    getData_current_date=getData_current_date.head(20)#\u53d6\u524d20\u4f4d\u80a1\u7968\n    \n    getData_last_date=DataAPI.MktStockFactorsOneDayGet(tradeDate=last_date,secID=list(getData_current_date.index),field=['secID','MACD','DEA','DIFF','NetProfitGrowRate'],pandas=\"1\")#\u83b7\u53d6\u5f53\u65e5\u80a1\u7968\u6c60\u4e2d\u7684MACD\uff0fDEA\uff0fDIFF\uff0f\u51c0\u5229\u6da6\u589e\u957f\u7387\uff08Net profit growth rate\uff09\u3002\u8ba1\u7b97\u65b9\u6cd5\uff1a\u51c0\u5229\u6da6\u589e\u957f\u7387=(\u4eca\u5e74\u51c0\u5229\u6da6\uff08TTM\uff09/\u53bb\u5e74\u51c0\u5229\u6da6\uff08TTM\uff09)-1\n    getData_last_date.set_index('secID',inplace=True)#\u8bbe\u7f6e\u7d22\u5f15\n    \n    for stock in list(getData_current_date.index):\n        if((getData_current_date['MACD'][stock]>0)&(getData_last_date['MACD'][stock]<0)):#MACD\u91d1\u53c9\n            buylist.append(stock)\n            #\u5982\u679cMACD\u91d1\u53c9\uff0c\u5219\u4e70\u5165\n    for stock in list(getData_current_date.index):\n        if((getData_current_date['MACD'][stock]<0)&(getData_last_date['MACD'][stock]>0)):#MACD\u6b7b\u53c9\n            selist.append(stock)\n            #\u5982\u679cMACD\u6b7b\u53c9\uff0c\u5219\u5356\u51fa\n    \n    for stock in selist:#\u904d\u5386\u6574\u4e2aselist\n        if(stock in account.valid_secpos):#\u5728\u539f\u6709\u8d26\u6237\u7684\u6709\u6548\u5934\u5bf8\u4e2d\u904d\u5386\u80a1\u7968\n            order_to(stock,0)#\u4ed3\u4f4d\u8c03\u6574\u81f30\n        else:\n            pass\n        \n    for stock in buylist:#\u904d\u5386\u6574\u4e2abuylist\n        if(stock in account.valid_secpos):#\u5728\u539f\u6709\u8d26\u6237\u7684\u6709\u6548\u5934\u5bf8\u4e2d\u904d\u5386\u80a1\u7968\n            pass\n        else:\n            order(stock,account.cash/account.referencePrice[stock]/len(buylist))#\u73b0\u6709\u73b0\u91d1\u5934\u5bf8/\u80a1\u7968\u6536\u76d8\u4ef7/\u80a1\u7968\u6570\u91cf", "language": "python", "metadata": {}, "outputs": [{"ename": "TypeError", "evalue": "integer argument expected, got float", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mTypeError\u001b[0m                                 Traceback (most recent call last)", "\u001b[1;32m<mercury-input-8-5159752348C5468F99549682E5DD02A0>\u001b[0m in \u001b[0;36m<module>\u001b[1;34m()\u001b[0m\n\u001b[0;32m     82\u001b[0m                                             \u001b[0mfreq\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0mfreq\u001b[0m\u001b[1;33m,\u001b[0m \u001b[0msecurity_base\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0msecurity_base\u001b[0m\u001b[1;33m,\u001b[0m \u001b[0msecurity_cost\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0msecurity_cost\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\u001b[0m\u001b[0m\n\u001b[0;32m     83\u001b[0m                                             \u001b[0mmax_history_window\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0mmax_history_window\u001b[0m\u001b[1;33m,\u001b[0m \u001b[0mpreload_data\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0m_QUARTZ_PRELOAD_DATA\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\u001b[0m\u001b[0m\n\u001b[1;32m---> 84\u001b[1;33m                                             display=True, return_quartz_data=True)\n\u001b[0m\u001b[0;32m     85\u001b[0m     \u001b[0m_QUARTZ_CACHE\u001b[0m\u001b[1;33m[\u001b[0m\u001b[1;34m'start'\u001b[0m\u001b[1;33m]\u001b[0m \u001b[1;33m=\u001b[0m \u001b[0mstart\u001b[0m\u001b[1;33m\u001b