erencePrice = account.get_history(1)['benchmark']\n    referenceChangeRate = referencePrice['return'][0]\n    stockPool =[]\n    for stock in universe:\n        try:\n            \n            #print stock\n            stockPrice = account.get_symbol_history(stock,1)\n            changeRate = stockPrice['closePrice'][0]/stockPrice['openPrice'][0]-1\n            # should be up\n            if changeRate < 0:\n                continue\n            upShadowRate = stockPrice['highPrice'][0]/stockPrice['closePrice'][0]-1\n            downShadowRate = 1 - stockPrice['lowPrice'][0]/stockPrice['openPrice'][0]\n            # shadow filter\n            # pass\n\n            # changeRate bigger than whole change Rate\n            if (changeRate < referenceChangeRate) or (changeRate+0.02 > referenceChangeRate):\n                continue\n            #print referenceChangeRate,changeRate,upShadowRate,downShadowRate\n            stockPool.append(stock)\n        except:\n            #print 'error'\n            pass\n    print stockPool\n    for stock in account.avail_secpos.keys():\n        if stock not in stockPool:\n            try:\n                order_to(stock,0)\n            except:\n                pass\n    \n    \n    stockCount = len(stockPool)\n    cash = account.cash \n    for stock in stockPool:\n        \n        order(stock,cash/stockCount)\n\n    \n    return\n", "language": "python", "metadata": {}, "outputs": [], "trusted": true}, {"cell_type": "code", "collapsed": false, "id": "56AA29EC9EEB48EF83D32E8CC48A3A16", "input": "", "language": "python", "metadata": {}, "outputs": [], "trusted": true}], "metadata": {}}]}