{"metadata": {"signature": "sha256:b10f14ac9594af72463644086747f79e3c0a4ede9cc3b977c2a401afadff4b25"}, "nbformat": 3, "nbformat_minor": 0, "worksheets": [{"cells": [{"cell_type": "markdown", "id": "154866987B6040F7BD6C98C0955A4300", "metadata": {}, "source": "# SINGLE_FACTOR"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "FFA2C28F50064C028A54221637682F33", "input": "import pandas as pd\nimport numpy as np\nimport matplotlib.pyplot as plt\n\n\n\n###stock pool----hs300\nhs300=DataAPI.IdxConsGet(secID=u\"\",ticker=u\"000300\",isNew=u\"\",intoDate=u\"\",field=u\"\",pandas=\"1\")\nhs300_new=hs300[hs300.isNew==1]\n\ntickers_pool=list(hs300.consTickerSymbol.values)\n#print(tickers_pool[0])\ntickers_pool2=[ticker+'.XSHE' for ticker in tickers_pool]\nhs300='000001'\nmonth_return=DataAPI.MktEqumGet(secID=u\"\",ticker=hs300,monthEndDate=u\"\",beginDate=u\"20100101\",endDate=u\"20180810\",field=['secID','endDate','return'],pandas=\"1\")\n\nall_return=DataAPI.MktEqumGet(secID=u\"\",ticker=tickers_pool,monthEndDate=u\"\",beginDate=u\"20100101\",endDate=u\"20180810\",field=['secID','endDate','return'],pandas=\"1\")\n####FACTORS TAB\n#print(tickers_pool)\ncandidate_factor=['DAVOL5','HBETA','PB','CRMA','CashRateOfSales','CashRateOfSales','BLEV','EquityFixedAssetRatio','EPS','QuickRatio''ASSI''TotalAssetsTRate']\nsingle_factor=candidate_factor[0]\nnew_factor=['ticker','tradeDate']\nnew_factor.append(single_factor)\n\nall_factor_list=[]\nfor each_ticker in tickers_pool:\n    factor_frame=DataAPI.MktStockFactorsDateRangeGet(secID=u\"\",ticker=each_ticker,beginDate=u\"20100101\",endDate=u\"20180810\",field=new_factor,pandas=\"1\")\n    factor_frame['ticker']=factor_frame['ticker'].map(lambda x:x+'.XSHE')\n    all_factor_list.append(factor_frame)\n \nall_factor_df=pd.concat(all_factor_list,axis=0)\nprint(all_factor_df.head(20))\nall_df=pd.merge(all_return,all_factor_df,left_on=['secID','endDate'],right_on=['ticker','tradeDate'],how='left')\nall_df_cleaned=all_df.dropna()\nall_df_cleaned=all_df_cleaned.drop(['ticker','tradeDate'],axis=1)\n\n\n\n\n############################################################################################################################################################\n######Backtesting stage\n##\u6309\u6b64\u5206\u6570\u52065\u7ec4\uff0c\u6bcf\u6708\u51fa\u8c03\u4ed3\uff0c\u7b97\u51fa5\u57282015-06-01\u52302017-06-30\u7684\u8868\u73b0\n#### Data configuration\nimport time\nfrom CAL.PyCAL import *\nstart_time=time.time()\n\nstart='2015-06-30'\nend='2016-06-30'\nbenchmark='HS300'\nuniverse=DynamicUniverse('HS300')\ncapital_base=10000000\nfred='d'\nrefresh_rate=Monthly(1)\naccounts = {'fantacy_account':AccountConfig(account_type='security',capital_base=capital_base)}\nnew_columns=['secID','endDate']\nnew_columns.append(single_factor)\n#print(new_columns)\n#print(all_df_cleaned.head(20))\n#rint(new_columns)\nfactor_data=all_df_cleaned[new_columns]\n\nfactor_data['endDate']=factor_data['endDate'].apply(lambda x:x.replace(\"-\",\"\"))\nfactor_data.rename(columns={single_factor:'factor'},inplace=True)\nfactor_data=factor_data.set_index('endDate',drop=False)\n#print(factor_data.head(20))\nfactor_data.dropna(inplace=True)\nq_dates=factor_data.index.values\nq_dates=list(set(q_dates))\n#print(q_dates)\n\n###-------------BACKTESTING-------------------------------------------------------------------------------------#\n####\u5728\u6bcf\u6708\u5e95\u628a\u80a1\u7968\u6309\u6b64\u6253\u5206\u5206\u4e09\u7ec4\uff0c\u5bf9\u7b2ci\u4e2asegment\u6bcf\u6b21\u7b49\u4ed3\u4f4d\u6301\u6709\u7b2ci\u7ec4\u7684\u80a1\u7968 \nsim_params=quartz.SimulationParameters(start,end,benchmark,universe,capital_base,refresh_rate=refresh_rate,accounts=accounts)\ndata=quartz.get_backtest_data(sim_params)\n#\u8fd0\u884c\u7ed3\u679c\nresults={}\nresult_new={}\nN=5\n###\u904d\u5386\u56de\u6d4b\nfor piece in range(1,N+1):\n    def initialize(context):\n        pass\n    \n    def handle_data(context):\n        account=context.get_account('fantacy_account')\n        current_universe=context.get_universe('stock',exclude_halt=True)\n        pre_date=context.previous_date.strftime(\"%Y%m%d\")\n       # print(pre_date)\n        if pre_date not in q_dates:\n            return\n                                           \n        q=factor_data.ix[pre_date].dropna()\n\n        q=q.set_index('secID',drop=False)\n        #print(type(q))\n        q=q.ix[current_universe]\n        #print(q)\n        q.dropna(inplace=True)\n        #print(q)\n        \n        \n        q_min=q['factor'].quantile((piece-1)*1.0/N)\n        q_max=q['factor'].quantile(piece*1.0/N) \n        #print(q_min) \n        my_univ=q[(q['factor']>=q_min)&(q['factor']<q_max)].index.values   \n       \n        \n    #trading part\n        positions=account.get_positions()\n        \n        #buy\n        buy_list=list(set(my_univ).difference(positions))\n        for stk in buy_list:\n            account.order_pct_to(stk,1.0/max(len(buy_list),1))\n         #sell   \n        sell_list=list(set(positions).difference(my_univ))\n       # print(['sell']+sell_list)\n        for stk in sell_list:\n            account.order_to(stk,0)  \n                                               \n    \n    ##\u751f\u6210\u7b56\u7565\u5bf9\u8c61                                            \n    strategy=quartz.TradingStrategy(initialize,handle_data)                                         \n    #\u5f00\u59cb\u56de\u6d4b   \n    bt,perf=quartz.quick_backtest(sim_params,strategy,data=data)\n                                                \n    ####\u4fdd\u5b58\u8fd0\u884c\u7ed3\u679c\n    results[piece]={'max_drawdown':perf['max_drawdown'],'sharpe':perf['sharpe'],'alpha':perf['alpha'],'annualized_return':perf['annualized_return'],'bt':bt}   \n  #  print(results[piece])                                     \n##############################\u753b\u56fe\u5c55\u793a\u7ed3\u679c    \n\nimport seaborn as sns\n\ndef draw_graphics(results):    \n    sns.set_style('white')\n\n    fig = plt.figure(figsize=(10,8))\n    fig.set_tight_layout(True)\n    ax1 = fig.add_subplot(211)\n    ax2 = fig.add_subplot(212)\n    ax1.grid()\n    ax2.grid()\n\n    for qt in results:\n        bt = results[qt]['bt']\n\n        data = bt[[u'tradeDate',u'portfolio_value',u'benchmark_return']]\n        data['portfolio_return'] = data.portfolio_value/data.portfolio_value.shift(1) - 1.0   # \u603b\u5934\u5bf8\u6bcf\u65e5\u56de\u62a5\u7387\n        data['portfolio_return'].ix[0] = data['portfolio_value'].ix[0]/\tcapital_base - 1.0\n        data['excess_return'] = data.portfolio_return - data.benchmark_return                 # \u603b\u5934\u5bf8\u6bcf\u65e5\u8d85\u989d\u56de\u62a5\u7387\n        data['excess'] = data.excess_return + 1.0\n        data['excess'] = data.excess.cumprod()                # \u603b\u5934\u5bf8\u5bf9\u51b2\u6307\u6570\u540e\u7684\u51c0\u503c\u5e8f\u5217\n        data['portfolio'] = data.portfolio_return + 1.0     \n        data['portfolio'] = data.portfolio.cumprod()          # \u603b\u5934\u5bf8\u4e0d\u5bf9\u51b2\u65f6\u7684\u51c0\u503c\u5e8f\u5217\n        data['benchmark'] = data.benchmark_return + 1.0\n        data['benchmark'] = data.benchmark.cumprod()          # benchmark\u7684\u51c0\u503c\u5e8f\u5217\n  \n        results[qt]['hedged_max_drawdown'] = max([1 - v/max(1, max(data['excess'][:i+1])) for i,v in enumerate(data['excess'])])  # \u5bf9\u51b2\u540e\u51c0\u503c\u6700\u5927\u56de\u64a4\n        results[qt]['hedged_volatility'] = np.std(data['excess_return'])*np.sqrt(252)\n        results[qt]['hedged_annualized_return'] = (data['excess'].values[-1])**(252.0/len(data['excess'])) - 1.0\n        ax1.plot(data['tradeDate'], data[['portfolio']], label=str(qt))\n        ax2.plot(data['tradeDate'], data[['excess']], label=str(qt))\n\n\n    ax1.legend(loc=0)\n    ax2.legend(loc=0)\n    ax1.set_ylabel(u\"\u51c0\u503c\", fontproperties=font, fontsize=16)\n    ax2.set_ylabel(u\"\u5bf9\u51b2\u51c0\u503c\", fontproperties=font, fontsize=16)\n    ax1.set_title(u\"\u56e0\u5b50\u4e0d\u540c\u5206\u4f4d\u6570\u5206\u7ec4\u9009\u80a1\u51c0\u503c\u8d70\u52bf\", fontproperties=font, fontsize=16)\n    ax2.set_title(u\"\u56e0\u5b50\u4e0d\u540c\u5206\u4f4d\u6570\u5206\u7ec4\u9009\u80a1\u5bf9\u51b2\u4e2d\u8bc1500\u6307\u6570\u540e\u51c0\u503c\u8d70\u52bf\", fontproperties=font, fontsize=16)\n\n    # results \u8f6c\u6362\u4e3a DataFrame\n    results_pd = pd.DataFrame(results).T.sort_index()\n\n    results_pd = results_pd[[u'alpha', u'annualized_return', u'max_drawdown',u'sharpe',  \n                             u'hedged_annualized_return', u'hedged_max_drawdown', u'hedged_volatility']]\n\n    cols = [(u'\u98ce\u9669\u6307\u6807', u'Alpha'), (u'\u7eaf\u80a1\u7968\u591a\u5934\u65f6', u'\u5e74\u5316\u6536\u76ca'),\n            (u'\u7eaf\u80a1\u7968\u591a\u5934\u65f6', u'\u6700\u5927\u56de\u64a4'), (u'\u7eaf\u80a1\u7968\u591a\u5934',u'shape_ratio'),(u'\u5bf9\u51b2\u540e', u'\u5e74\u5316\u6536\u76ca'), (u'\u5bf9\u51b2\u540e', u'\u6700\u5927\u56de\u64a4'), (u'\u5bf9\u51b2\u540e', u'\u6536\u76ca\u6ce2\u52a8\u7387')]\n    results_pd.columns = pd.MultiIndex.from_tuples(cols)\n    results_pd.index.name = u'\u4e09\u5206\u4f4d\u7ec4\u522b'\n    \n    print(results_pd)\n    return results_pd\n\n \ndraw_graphics(results)\n\n", "language": "python", "metadata": {}, "outputs": [], "trading_days": "", "trusted": true}, {"cell_type": "code", "collapsed": false, "id": "940599C7FE554C758BB42F1AD65FCA74", "input": "", "language": "python", "metadata": {}, "outputs": [], "trusted": true}], "metadata": {}}]}