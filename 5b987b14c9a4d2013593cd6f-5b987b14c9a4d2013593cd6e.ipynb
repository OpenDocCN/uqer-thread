at32(tmp)\n    e=[0]*10\n    e[0]=tmp[0]\n    for i in range(9):\n        e[i+1]=np.float32((2.0/11)*tmp[i+1]+(9.0/11)*e[i])\n    return e[-1]\n# \u6bcf\u4e2a\u5355\u4f4d\u65f6\u95f4(\u5982\u679c\u6309\u5929\u56de\u6d4b,\u5219\u6bcf\u5929\u8c03\u7528\u4e00\u6b21,\u5982\u679c\u6309\u5206\u949f,\u5219\u6bcf\u5206\u949f\u8c03\u7528\u4e00\u6b21)\u8c03\u7528\u4e00\u6b21\ndef handle_data(context):    \n    xy=context.get_account('fantasy_account')\n    data=context.history(symbol=universe, attribute=['openPrice','closePrice'], time_range=10, freq='1d', style='sat', rtype='frame')\n    aOpen=data['SRM0']['openPrice']\n    bClose=data['SRM0']['closePrice']\n    \n    Dif=EMA(bClose)-EMA(aOpen)\n    tmp=context.get_universe('futures')\n     \n    target=(context.get_symbol(tmp))\n    #print(target[0])\n    print(Dif)\n    if (10>Dif>=4):\n        xy.order(target[0],10,'open')\n    else:\n        xy.close_all_positions()\n    ", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "-13.5347"}, {"output_type": "stream", "stream": "stdout", "text": "\n"}, {"output_type": "stream", "stream": "stdout", "text": "-1.88623\n-5.87695\n-11.8755\n-16.1338\n-17.8032\n-46.2046\n-23.9502\n-21.0034\n-29.9282\n-22.9048\n-20.4873\n-43.5713\n-33.7681\n-19.791\n-33.3223"}, {"output_type": "stream", "stream": "stdout", "text": "\n-5.02734\n-6.4707\n-11.375\n-8.44287\n-6.38574\n"}, {"output_type": "stream", "stream": "stdout", "text": "-27.5137\n-5.75537\n-12.8994\n-18.7192\n-14.7075\n-19.8115\n-25.5698\n-38.7163\n-27.3604\n-16.3164\n-8.38281\n-2.16406\n-2.22266\n3.59961\n-18.085\n-22.0146"}, {"output_type": "stream", "stream": "stdout", "text": "\n-19.333\n-0.0595703\n7.57373\n5.5459\n"}, {"output_type": "stream", "stream": "stdout", "text": "27.3936\n9.93018\n-8.76221\n-18.1528\n4.93652\n18.27\n12.3643\n9.75928\n12.0415\n11.9014\n-1.05078\n-19.0903\n-29.8931\n-8.78662"}, {"output_type": "stream", "stream": "stdout", "text": "\n-23.7119\n-16.80