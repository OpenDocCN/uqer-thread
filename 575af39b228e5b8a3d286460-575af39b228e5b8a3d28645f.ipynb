u6570\nmacd_line  = 9     # DEA\u7ebf\u5e73\u6ed1\u5929\u6570\nq_macd = 10 #\u6743\u91cd\n\n## \u4f7f\u7528talib\u8ba1\u7b97RSI\u7684\u53c2\u6570\ns_line_rsi = 34    # \u77ed\u671fRSI\nm_line_rsi = 12    # \u4e2d\u671fRSI\nl_line_rsi  = 24    # \u957f\u671fRSI\nup_rsi = 70 # \u8d85\u5356\u754c\u9650\nlow_rsi = 30 # \u8d85\u4e70\u754c\u9650\nq_rsi = 10 #\u6743\u91cd\n\n## \u4f7f\u7528talib\u8ba1\u7b97CCI\u7684\u53c2\u6570\nN_cci = 14    # \u8ba1\u7b97\u5468\u671f\nup_cci = 100 # \u8d85\u5356\u754c\u9650\nlow_cci = -100 # \u8d85\u4e70\u754c\u9650\nlongest_history = 150\nq_cci = 10 #\u6743\u91cd\n\n## \u4f7f\u7528talib\u8ba1\u7b97OBV\u7684\u53c2\u6570\nq_obv = 10 #\u6743\u91cd\n\ndef initialize(account):                   # \u521d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    # account.universe=universe\n    pass\n\ndef handle_data(account): # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    all_close_prices = account.get_attribute_history('closePrice', longest_history)\n    all_high_prices = account.get_attribute_history('highPrice', longest_history)\n    all_low_prices = account.get_attribute_history('lowPrice', longest_history)\n    all_close_volume = account.get_attribute_history('turnoverVol', longest_history)\n    golden_bucket=[]\n    dead_bucket=[]\n    xh=[0,0,0,0,0]\n    zt=[0,0,0,0,0]\n    sum_state=0\n    sum_signal=0\n    for stk in account.universe:\n        close_prices = all_close_prices[stk]\n        high_prices = all_high_prices[stk]\n        low_prices = all_low_prices[stk]\n        volume = all_close_volume[stk]\n        if close_prices is None or high_prices is None or low_prices is None or volume is None:\n            continue\n        try:\n            # talib\u8ba1\u7b97MACD\n            macd_temp = talib.MACD(close_prices, fastperiod=s_line_macd, slowperiod=l_line_macd, signalperiod=macd_line)\n            DIF = macd_temp[0]\n            DEA = macd_temp[1]\n            MACD = 2*macd_temp[2]\n             # talib\u8ba1\u7b97RSI\n            RSI = talib.RSI(close_prices,s_line_rsi)          \n             # talib\u8ba1\u7b97cci\n            CCI = talib.CCI(high_prices, low_prices, close_prices,timeperiod = N_cci)\n            # talib\u8ba1\u7b97OBV\n            OBV = talib.OBV(close_prices,volume) \n            # talib\u8ba1\u7b97kd\n            K,D = talib.STOCH(high_prices, low_prices, close_prices, fastk_period=N_kd, slowk_matype=0, slowk_period=M1_kd, slowd_period=M2_kd)\n#matype: 0=SMA, 1=EMA, 2=WMA, 3=DEMA, 4=TEMA, 5=TRIMA, 6=KAMA, 7=MAMA, 8=T3 (Default=SMA)\n           \n    #\u5404\u4e2a\u6280\u672f\u6307\u6807\u4fe1\u53f7\u6807\u8bb0\n            if K[-2] < D[-2] and K[-1] > D[-1] and K[-1] > up_kd:\n                xh[0] = 2\n            if K[-2] < D[-2] and K[-1] > D[-1] and K[-1] < up_kd:\n                xh[0] = 1\n            if K[-2] > D[-2] and K[-1] < D[-1] and K[-1] < up_kd:\n                xh[0] = -1\n            \n            if MACD[-2] < 0 and MACD[-1] > 0 and DIF[-1] > 0:\n                xh[1] = 2\n            if MACD[-2] < 0 and MACD[-1] > 0 and DIF[-1] < 0:\n                xh[1] = 1\n            if MACD[-2] > 0 and MACD[-1] < 0:\n                xh[1] = -1\n            \n#          if OBV[-1] > OBV[-2]:\n#              xh[2] = 1\n#          if OBV[-1] > OBV[-2]:\n#              xh[2] = -1\n            \n#          if RSI[-2] < up_rsi and RSI[-1] > up_rsi:\n#              xh[3] = 1\n            \n            if (CCI[-2] < up_cci and CCI[-1] > up_cci) or (CCI[-2] < low_cci and CCI[-1] > low_cci):\n                xh[4] = 1\n            if (CCI[-2] > up_cci and CCI[-1] < up_cci) or (CCI[-2] > low_cci and CCI[-1] < low_cci):\n                xh[4] = -1\n            \n                #\u5404\u4e2a\u6280\u672f\u6307\u6807\u72b6\u6001\u6807\u8bb0\n            if K[-1] > D[-1]:\n                zt[0] = 1\n            if K[-1] < D[-1]:\n                zt[0] = -1\n            \n            if MACD[-1] > 0:\n                zt[1] = 1\n            if MACD[-1] < 0:\n                zt[1] = -1\n            \n            if OBV[-1] > OBV[-2]:\n                zt[2] = 1\n            if OBV[-1] < OBV[-2]:\n                zt[2] = -1\n            \n            if RSI[-1] > up_rsi:\n                zt[3] = 1\n            if RSI[-1] < low_rsi:\n                zt[3] = -1\n            \n            if CCI[-1] > up_cci:\n                zt[4] = 1\n            if CCI[-1] < low_cci:\n                zt[4] = -1\n\n            sum_signal = xh[0] * q_kd+xh[1] * q_macd+xh[2] * q_obv+xh[3] * q_rsi+xh[4] * q_cci  \n            sum_state = zt[0] * q_kd+zt[1] * q_macd+zt[2] * q_obv+zt[3] * q_rsi+zt[4] * q_cci\n        \n        except:\n            continue\n        if  sum_signal > 0 and sum_state > 0:  \n             golden_bucket.append(stk)\n          \n        if  sum_signal < 0 and sum_state < 0:\n             dead_bucket.append(stk)\n\n    for stk in account.valid_secpos:\n        if stk in dead_bucket:\n                order_pct(stk,-1)  \n    for stk in golden_bucket:\n        order_pct(stk,0.1)", "language": "python", "metadata": {}, "outputs": [{"ename": "ValueError", "evalue": "No valid reference price of 601579.XSHG!", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mValueError\u001b[0m                                Traceback (most recent call last)", "\u001b[1;32m<mercury-input-26-3afa7850d267>\u001b[0m in \u001b[0;36m<module>\u001b[1;34m()\u001b[0m\n\u001b[0;32m    195\u001b[0m                                                           \u001b[0mslippage\u001b[0m \u001b[1;33m=\u001b[0m \u001b[0mslippage\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\u001b[0m\u001b[0m\n\u001b[0;32m    196\u001b[0m                                                           \u001b[0mrefresh_rate\u001b[0m \u001b[1;33m=\u001b[0m \u001b[0mrefresh_rate\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\u001b[0m\u001b[0m\n\u001b[1;32m--> 197\u001b[1;33m        