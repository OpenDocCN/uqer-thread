   # \u7b56\u7565\u53c2\u8003\u6807\u51c6\nuniverse = set_universe('HS300')           # \u8bc1\u5238\u6c60\uff0c\u652f\u6301\u80a1\u7968\u548c\u57fa\u91d1\ncapital_base = 1000000                      # \u8d77\u59cb\u8d44\u91d1\nfreq = 'm'                                 # \u7b56\u7565\u7c7b\u578b\uff0c'd'\u8868\u793a\u65e5\u95f4\u7b56\u7565\u4f7f\u7528\u65e5\u7ebf\u56de\u6d4b\uff0c'm'\u8868\u793a\u65e5\u5185\u7b56\u7565\u4f7f\u7528\u5206\u949f\u7ebf\u56de\u6d4b\nrefresh_rate = 1                           # \u8c03\u4ed3\u9891\u7387\uff0c\u8868\u793a\u6267\u884chandle_data\u7684\u65f6\u95f4\u95f4\u9694\uff0c\u82e5freq = 'd'\u65f6\u95f4\u95f4\u9694\u7684\u5355\u4f4d\u4e3a\u4ea4\u6613\u65e5\uff0c\u82e5freq = 'm'\u65f6\u95f4\u95f4\u9694\u4e3a\u5206\u949f\nday_count = {}\nfor stk in universe:\n    day_count[stk] = 0\n\ndef initialize(account):                   # \u521d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    pass\n\ndef handle_data(account):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    \n    for stk in account.valid_seccost:\n        if account.referencePrice[stk] == 0 or np.isnan(account.referencePrice[stk]):\n            continue\n        if day_count[stk] > 2401 or account.valid_seccost[stk] * 1.1 < account.referencePrice[stk]: \n            order_to(stk,0)\n            day_count[stk] = 0\n \n    if  account.current_minute == '11:29' and account.cash > 150000: \n        buylist = []\n        windows = int(account.current_minute[0:2])*60 + int(account.current_minute[3:5]) - 570 #\u8ba1\u7b97\u8ddd\u79bb\u5f00\u76d8\u7684\u5206\u949f\u6570\n        eq_volwm = account.get_attribute_history('turnoverVol',windows)\n        eq_closepricewm = account.get_attribute_history('closePrice',windows)\n        \n        eq_closeprice1d = account.get_daily_attribute_history('closePrice',1)\n        \n        eq_vol7d = account.get_daily_attribute_history('turnoverVol',7)\n        eq_volwm = pd.DataFrame(eq_volwm).sum()\n        \n        # closeprice_20 = account.get_daily_attribute_history('closePrice',20)\n        # closeprice_20 = pd.DataFrame(closeprice_20)\n        # eq20_ret = closeprice_20.ix[len(closeprice_20)-1] / closeprice_20.ix[0]\n        \n        dif_vol =  pd.DataFrame(eq_vol7d).mean() * 0.3  - eq_volwm\n        buy_list = dif_vol[dif_vol > 0].index\n\n        total_money = account.referencePortfolioValue\n        prices = account.referencePrice\n        \n        for stk in buy_list:\n            if eq_closepricewm[stk][-1] > eq_closepricewm[stk][0] * 1.02 and prices[stk] / eq_closeprice1d[stk][0] < 1.098: #and eq20_ret[stk] < eq20_ret.quantile(0.3): #\u6da8\u505c\u4e86\u7684\u5c31\u522b\u4e0b\u5355\u4e86 \n                buylist.append(stk)\n        \n        for stk in buylist:\n            if stk not in account.valid_secpos:\n                if np.isnan(prices[stk]) or prices[stk] == 0:  # \u505c\u724c\u6216\u662f\u8fd8\u6ca1\u6709\u4e0a\u5e02\u7b49\u539f\u56e0\u4e0d\u80fd\u4ea4\u6613\n                    continue\n                order_to(stk,0.2*total_money / prices[stk]) #\u7b49\u6743\u4e70\u5165\n                \n    for stk in account.valid_secpos:\n        day_count[stk] = day_count[stk] + 1", "language": "python", "metadata": {}, "outputs": [{"metadata": {}, "output_type": "pyout", "prompt_number": 26, "text": "'{\"information\": 2.24122, \"benchmark_cumulative_return\": {\"1445299200000\": -0.0747878145, \"1461801600000\": -0.1826576845, \"1455753600000\": -0.2102979131, \"1439424000000\": 0.0539354522, \"1449446400000\": -0.0463660814, \"1446422400000\": -0.1010985544, \"1459468800000\": -0.1668015723, \"1440720000000\": -0.1356678476, \"1447718400000\": -0.0280620135, \"1450656000000\": -0.0002417958, \"1459382400000\": -0.1677860819, \"1456963200000\": -0.2090765212, \"1449187200000\": -0.0489560113, \"1440028800000\": -0.0272688717, \"1451952000000\": -0.1003698053, \"1444867200000\": -0.0982919134, \"1453161600000\": -0.1664834881, \"146119680000