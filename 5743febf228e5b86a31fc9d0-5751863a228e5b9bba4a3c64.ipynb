\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    \n    for stk in account.valid_seccost:\n        if account.referencePrice[stk] == 0 or np.isnan(account.referencePrice[stk]):\n            continue\n        if day_count[stk] > 2401 or account.valid_seccost[stk] * 1.1 < account.referencePrice[stk]: \n            order_to(stk,0)\n            day_count[stk] = 0\n \n    if  account.current_minute > '09:31' and account.current_minute < '11:30' and account.cash > 150000: \n        buylist = []\n        windows = int(account.current_minute[0:2])*60 + int(account.current_minute[3:5]) - 570 #\u8ba1\u7b97\u8ddd\u79bb\u5f00\u76d8\u7684\u5206\u949f\u6570\n        eq_volwm = account.get_attribute_history('turnoverVol',windows)\n        eq_closepricewm = account.get_attribute_history('closePrice',windows)\n        \n        eq_closeprice1d = account.get_daily_attribute_history('closePrice',1)\n        \n        eq_vol7d = account.get_daily_attribute_history('turnoverVol',7)\n        eq_volwm = pd.DataFrame(eq_volwm).sum()\n        \n        closeprice_60 = account.get_daily_attribute_history('closePrice',60)\n        closeprice_60 = pd.DataFrame(closeprice_60)\n        eq60_ret = closeprice_60.ix[len(closeprice_60)-1] / closeprice_60.ix[0]\n        #print eq60_ret\n        \n        closeprice_10  = account.get_daily_attribute_history('closePrice',10)\n        closeprice_10 = pd.DataFrame(closeprice_10)\n        eq10_ret = closeprice_10.ix[len(closeprice_10)-1] / closeprice_10.ix[0]\n        #print eq3_ret\n        \n        dif_vol =  pd.DataFrame(eq_vol7d).mean() * 1.5  - eq_volwm\n        buy_list = dif_vol[dif_vol < 0].index\n\n        total_money = account.referencePortfolioValue\n        prices = account.referencePrice\n        \n        for stk in buy_list:\n            \n            if eq_closepricewm[stk][-1] > np.min(eq_closepricewm[stk]) * 1.07 and prices[stk] / eq_closeprice1d[stk][0] < 1.095 and eq60_ret[stk] < eq60_ret.quantile(0.3) and eq10_ret[stk] < 1: #\u6da8\u505c\u4e86\u7684\u5c31\u522b\u4e0b\u5355\u4e86 \n                #print stk,eq3_ret[stk]\n                buylist.append(stk)\n        \n        for stk in buylist:\n            if stk not in account.valid_secpos:\n                if np.isnan(prices[stk]) or prices[stk] == 0:  # \u505c\u724c\u6216\u662f\u8fd8\u6ca1\u6709\u4e0a\u5e02\u7b49\u539f\u56e0\u4e0d\u80fd\u4ea4\u6613\n                    continue\n                order_to(stk,0.2*total_money / prices[stk]) #\u7b49\u6743\u4e70\u5165\n                \n    for stk in account.valid_secpos:\n        day_count[stk] = day_count[stk] + 1", "language": "python", "metadata": {}, "outputs": [{"metadata": {}, "output_type": "pyout", "prompt_number": 51, "text": "'{\"information\": 0.39539, \"benchmark_cumulative_return\": {\"1445299200000\": -0.0631173026, \"1461801600000\": -0.1723478297, \"1455753600000\": -0.2003367087, \"1439424000000\": 0.0672296635, \"1449446400000\": -0.0343370612, \"1446422400000\": -0.0897599229, \"1459468800000\": -0.1562917105, \"1440720000000\": -0.1247652689, \"1447718400000\": -0.0158021081, \"1450656000000\": 0.0123690304, \"1459382400000\": -0.1572886386, \"1456963200000\": -0.1990999104, \"1449187200000\": -0.0369596602, \"1440028800000\": -0.0149989617, \"1451952000000\": -0.0890219814, \"1444867200000\": -0.0869178793, \"1453161600000\": -0.155969614, \"1451433600000\": -0.0140245542, \"1461196800000\": -0.1723420686, \"1461542400000\": -0.1719675987, \"1450828800000\": 0.0124779672, \"1444694400000\": -0.0978565916, \"1454284800000\": -0.2403109829, \"1453334400000\": -0.193097131, \"1445212800000\": -0.0745140205, \"1442448000000\": -0.1523364699, \"1446768000000\": -0.006640164, \"1458172800000\": -0.1818737691, \"1441843200000\": -0.120765249, \"1456704000000\": -0.2464860709, \"1442534400000\": -0.148598579, \"1438560000000\": 0.002750652, \"1450396800000\": -0.0133075621, \"1450310400000\": -0.016456514, \"1458777600000\": -0.1667779165, \"1442880000000\": -0.125617908