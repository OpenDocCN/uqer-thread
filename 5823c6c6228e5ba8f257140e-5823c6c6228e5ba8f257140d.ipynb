 range(timerange):\n        dif_close['tag'][i]=math.modf(float(dif_close[s][i]))[1]\n    tag_list=list(dif_close['tag'])\n    lenth=len(tag_list)\n    temp=[]\n    tagend=tag_list[k:-1]\n    for i in range(lenth-k):\n        temp.append(tag_list[i:i+k])\n    clf=svm.SVC(C=0.8,kernel='linear',shrinking=False).fit(np.array(temp[:-2]), np.array(tagend[:-1]))\n    print(clf.score(np.array(temp[:-2]), np.array(tagend[:-1])))\n    pred=clf.predict(np.array(temp[-1]))\n    pred=[]\n    return len(pred)\n\ndef handle_data(account):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    buy_list=[]\n    sell_list=[]\n    for s in account.universe:\n        data=account.history(attribute= ['preClosePrice','closePrice'], symbol=str(s), time_range=timerange, freq='d')\n        close=data['closePrice']\n        pclose=data['preClosePrice']\n        dif_close=(close-pclose)/pclose*100\n        dif_close['tag']=999\n        for i in range(timerange):\n            dif_close['tag'][i]=math.modf(float(dif_close[s][i]))[1]\n        tag_list=list(dif_close['tag'])\n        lenth=len(tag_list)\n        temp=[]\n        tagend=tag_list[k:-1]\n        for i in range(lenth-k):\n            temp.append(tag_list[i:i+k])\n        clf=svm.SVC(C=0.08,kernel='linear',shrinking=False).fit(np.array(temp[:-2]), np.array(tagend[:-1]))\n        #print(clf.score(np.array(temp[:-2]), np.array(tagend[:-1])))\n        pred=clf.predict(np.array(temp[-1]))\n        #print pred\n        if pred>=1:\n            buy_list.append(s)\n        else:\n            if pred<0:\n                sell_list.append(s)\n    for st in sell_list:\n        if st in account.avail_security_position:\n            order_to(st,0)\n    for stt in buy_list:\n        order_pct_to(stt,0.4)", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stderr", "text": "-c:53: SettingWithCopyWarning: \nA value is trying to be set on a copy of a slice from a DataFrame