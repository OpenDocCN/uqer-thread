u663e\u793a\u7684\u540d\u5b57\u662f\u6700\u65b0\uff08\u73b0\u5b9e\u4e16\u754c\uff09\u7684\u540d\u5b57\uff0c\u800c\u4e0d\u662f\u56de\u6d4b\u5230\u90a3\u4e2a\u65f6\u70b9\u7684\u540d\u5b57\n    # \u6240\u4ee5\u6709\u65f6\u5019\u5373\u4f7f\u7528\u4e86\u4e0a\u9762\u8fd9\u4e2a\u5254\u9664ST\u80a1\u7684\u65b9\u6cd5\uff0c\u4f46\u8fd8\u662f\u80fd\u5728\u56de\u6d4b\u8be6\u60c5\u91cc\u770b\u5230\u6709ST\u80a1\uff0c\u56e0\u4e3a\u5b83\u4eec\u540e\u6765\u88abST\u4e86\uff08\u5f3a\u8feb\u75c7\u54c8\u54c8\uff09\n    # \u5e9f\u8bdd\u4e86\u8fd9\u4e48\u591a\uff0c\u7528\u4e0d\u7528\u4e0b\u9762\u8fd9\u6bb5\u5c31\u770b\u4e2a\u4eba\u5427\u54c8\u54c8\n    \n    df_ST = DataAPI.EquGet(secID=univ,field=u\"secID,secShortName\",pandas=\"1\")\n    STlist = list(df_ST.loc[df_ST.secShortName.str.contains('\u9000'), 'secID'])\n    univ = [s for s in univ if s not in STlist]\n    \n    # 3. \u53bb\u9664\u6d41\u52a8\u6027\u5dee\u7684\u80a1\u7968\n    tv = account.get_attribute_history('turnoverValue', 20)\n    mtv = {sec: sum(tvs)/20. for sec, tvs in tv.iteritems()}\n    univ = [s for s in univ if mtv.get(s, 0) >= 10**7]\n\n    # 4. \u53bb\u9664\u65b0\u4e0a\u5e02\u6216\u590d\u724c\u7684\u80a1\u7968\n    opn = account.get_attribute_history('openPrice', 1)\n    univ = [s for s in univ if not (np.isnan(opn.get(s, 0)[0]) or opn.get(s, 0)[0] == 0)]\n    \n    return univ\n\ndef initialize(context):\n    pass\n\ndef handle_data(context):\n    date = context.current_date\n    #yesterday = cal.advanceDate(date, '-1B', BizDayConvention.Following)\n    #yesterday = datetime(yesterday.year(), yesterday.month(), yesterday.dayOfMonth()).strftime('%Y%m%d'),\n\n    #STlist = DataAPI.SecSTGet(secID=context.get_universe(exclude_halt = True), beginDate=date, endDate=date, field=['secID'])['secID'].tolist()\n    universe = context.get_universe(exclude_halt = True)\n    universe = univClear(universe, context, date)\n\n    account = context.get_account('fantasy_account')\n    account.close_all_positions()\n    for stock in universe:\n        account.order_pct_to(stock, 1/float(len(universe)))", "language": "python", "metadata": {}, "outputs": [], "trading_days": ""}], "metadata": {}}]}