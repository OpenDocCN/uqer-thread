{"metadata": {"signature": "sha256:c298798cc28623f7ca98b42f4341936c286ccf790a1470017f4f3c6255df1e88"}, "nbformat": 3, "nbformat_minor": 0, "original_thread": "55ff6ce9f9f06c597265ef04", "worksheets": [{"cells": [{"cell_type": "markdown", "id": "D861EDB5969E422EAB8AA5071ADE7CC2", "metadata": {}, "source": "# \u56e0\u5b50\u9009\u80a1\u7b56\u7565-\u6253\u5206\u6cd5-Copy0"}, {"cell_type": "strategy", "collapsed": false, "folded": false, "has_detail": true, "id": "F2B3F450E7944B838D6E9AFF2565CFD6", "input": "# \u5bfc\u5165\u5305\nfrom CAL.PyCAL import *\nimport numpy as np\nimport pandas as pd\n\n# \u6784\u5efa\u65e5\u671f\u5217\u8868\uff0c\u4ee5\u4fdd\u8bc1\u6bcf\u6708\u7b2c\u4e00\u4e2a\u4ea4\u6613\u65e5\u5efa\u4ed3\ndata=DataAPI.TradeCalGet(exchangeCD=u\"XSHG\",beginDate=u\"20070101\",field=['calendarDate','isWeekEnd','isMonthEnd'],pandas=\"1\")\ndata = data[data['isMonthEnd'] == 1]\ndate_list = map(lambda x: x[0:4]+x[5:7]+x[8:10], data['calendarDate'].values.tolist())\n\nstart = '2007-01-01'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2017-07-10'                        # \u56de\u6d4b\u7ed3\u675f\u65f6\u95f4\nuniverse = set_universe('HS300') + set_universe('ZZ500')   # \u80a1\u7968\u6c60\nbenchmark = 'HS300'                       # \u7b56\u7565\u53c2\u8003\u6807\u51c6\ncapital_base = 10000000                     # \u8d77\u59cb\u8d44\u91d1\nfreq = 'd'                              # \u7b56\u7565\u7c7b\u578b\uff0c'd'\u8868\u793a\u65e5\u95f4\u7b56\u7565\u4f7f\u7528\u65e5\u7ebf\u56de\u6d4b\uff0c'm'\u8868\u793a\u65e5\u5185\u7b56\u7565\u4f7f\u7528\u5206\u949f\u7ebf\u56de\u6d4b\nrefresh_rate = 1                          # \u8c03\u4ed3\u9891\u7387\ncommission = Commission(buycost=0.0008, sellcost=0.0008)\n\n# \u65e5\u671f\u5904\u7406\u76f8\u5173\ncal = Calendar('China.SSE')\nperiod = Period('-1B')\nfactor_name = ['PB','FY12P','FinancialExpenseRate']\n\ndef initialize(account):                   # \u521d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    pass\n\ndef handle_data(account):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    \n    today = account.current_date\n    today = Date.fromDateTime(account.current_date)  # \u5411\u524d\u79fb\u52a8\u4e00\u4e2a\u5de5\u4f5c\u65e5\n    yesterday = cal.advanceDate(today, period)\n    yesterday = yesterday.toDateTime().strftime('%Y%m%d')\n    if yesterday in date_list:\n        \n        Factor1 = DataAPI.MktStockFactorsOneDayGet(tradeDate=yesterday,secID=account.universe[:400],field=['secID','PB','FY12P','FinancialExpenseRate'],pandas=\"1\")\n        Factor2 = DataAPI.MktStockFactorsOneDayGet(tradeDate=yesterday,secID=account.universe[400:],field=['secID','PB','FY12P','FinancialExpenseRate'],pandas=\"1\")\n        Factor = pd.concat([Factor1, Factor2])\n        Factor['ticker'] = Factor['secID'].apply(lambda x: x[0:6])\n        Factor.set_index('ticker',inplace=True)\n        \n        # \n        Factor['PB'] = 1.0 / Factor['PB']  #\n        factor = Factor['PB'].dropna().to_dict()\n        signal_PB = standardize(neutralize(winsorize(factor),yesterday))   # \u56e0\u5b50\u5904\u7406\n        \n        # \n        Factor['FY12P'] = Factor['FY12P']  # \n        factor = Factor['FY12P'].dropna().to_dict()\n        signal_FY12P = standardize(neutralize(winsorize(factor),yesterday))  # \u56e0\u5b50\u5904\u7406\n        \n        # \n        Factor['FinancialExpenseRate'] = 1.0 / Factor['FinancialExpenseRate']  # \n        factor = Factor['FinancialExpenseRate'].dropna().to_dict()\n        signal_FinancialExpenseRate = standardize(neutralize(winsorize(factor),yesterday))  # \u56e0\u5b50\u5904\u7406\n        # \u6784\u5efa\u7ec4\u5408score\u77e9\u9635\n        Total_Score = pd.DataFrame(index=Factor.index, columns=factor_name, data=0)\n        Total_Score['PB'][signal_PB.keys()] = signal_PB.values()\n        Total_Score['FY12P'][signal_FY12P.keys()] = signal_FY12P.values()\n        Total_Score['FinancialExpenseRate'][signal_FinancialExpenseRate.keys()]= signal_FinancialExpenseRate.values()\n        Total_Score['total_score'] = np.dot(Total_Score, np.array([1,1,1]))  # \u7efc\u5408\u4e24\u4e2a\u56e0\u5b50\u7684\u5927\u5c0f\uff0c\u4e0d\u5931\u4e00\u822c\u6027\uff0c\u7b49\u6743\u6c42\u548c\n        total_score = Total_Score['total_score'].to_dict()\n        \n        wts = simple_long_only(total_score,yesterday)  # \u7ec4\u5408\u6784\u5efa\u51fd\u6570\n        Factor['wts'] = np.nan\n        Factor['wts'][wts.keys()] = wts.values()\n        Factor = Factor[~np.isnan(Factor['wts'])]\n        Factor.set_index('secID', inplace=True)\n        Factor.drop(factor_name, axis=1, inplace=True)\n        \n        # \u5148\u5356\u51fa\n        sell_list = account.valid_secpos\n        for stk in sell_list:\n            order_to(stk, 0)\n\n        # \u518d\u4e70\u5165\n        buy_list = Factor.index\n        total_money = account.referencePortfolioValue\n        prices = account.referencePrice \n        for stk in buy_list:\n            if np.isnan(prices[stk]) or prices[stk] == 0:  # \u505c\u724c\u6216\u662f\u8fd8\u6ca1\u6709\u4e0a\u5e02\u7b49\u539f\u56e0\u4e0d\u80fd\u4ea4\u6613\n                continue\n            order(stk, int(total_money * Factor.loc[stk]['wts'] / prices[stk] /100)*100)\n    else:\n        return", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "\u5f00\u59cb\u52a0\u8f7d\u6570\u636e"}, {"output_type": "stream", "stream": "stdout", "text": "\n"}, {"output_type": "stream", "stream": "stdout", "text": "\u5f00\u59cb\u8fdb\u884c\u56de\u6d4b\n"}, {"output_type": "stream", "stream": "stdout", "text": "Warning: account.valid_secpos is deprecated, please use account.security_position instead.\nWarning: account.referencePortfolioValue is deprecated, please use account.reference_portfolio_value instead.\nWarning: account.referencePrice is deprecated, please use account.reference_price instead.\n"}, {"ename": "MFOutputError", "evalu