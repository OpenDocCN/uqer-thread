{"metadata": {"signature": "sha256:7491670490c24c62b89d489461dabd171671a03979705f57a4cee724e4f8a7a6"}, "nbformat": 3, "nbformat_minor": 0, "worksheets": [{"cells": [{"cell_type": "markdown", "id": "521F3DEACFF4405E91BC2DB9DDC43333", "metadata": {}, "source": "# \u8d85\u77ed\u7b56\u7565"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "8F409E2F9FAE4363ABCDD3DFBABC3211", "input": "#\u6da8\u505c\u5e03\u6797\u5e26\u5c0f\u65f6\u7b56\u7565\nimport numpy as np\nimport pandas as pd\nfrom CAL.PyCAL import *\nimport time\nimport datetime\nstart = '2017-12-11'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2017-12-12'                         # \u56de\u6d4b\u7ed3\u675f\u65f6\u95f4\nuniverse =set_universe('A') #DynamicUniverse('HS300')        # \u8bc1\u5238\u6c60\uff0c\u652f\u6301\u80a1\u7968\u548c\u57fa\u91d1\u3001\u671f\u8d27\nbenchmark = 'HS300'                        # \u7b56\u7565\u53c2\u8003\u57fa\u51c6\nfreq = 'm'                                 # 'd'\u8868\u793a\u4f7f\u7528\u65e5\u9891\u7387\u56de\u6d4b\uff0c'm'\u8868\u793a\u4f7f\u7528\u5206\u949f\u9891\u7387\u56de\u6d4b\nrefresh_rate = 15                          # \u6267\u884chandle_data\u7684\u65f6\u95f4\u95f4\u9694\n\naccounts = {\n    'fantasy_account': AccountConfig(account_type='security', capital_base=10000000)\n}\n\ndef initialize(context):                   # \u521d\u59cb\u5316\u7b56\u7565\u8fd0\u884c\u73af\u5883\n    pass\n\ndef handle_data(context):                  # \u6838\u5fc3\u7b56\u7565\u903b\u8f91\n    account = context.get_account('fantasy_account')\n    \n    list_chu=[]\n    for n in range(1,5):\n        x=str(-n)+'B'\n        x1=str(-n-1)+'B'\n        dt = Date.fromDateTime(context.current_date)\n        cal = Calendar('China.SSE')\n        last_day = cal.advanceDate(dt,x,BizDayConvention.Preceding)\n        last_day = last_day.strftime(\"%Y%m%d\")\n        last_day1=cal.advanceDate(dt,x1,BizDayConvention.Preceding)\n        last_day1=last_day1.strftime(\"%Y%m%d\")\n        df_price = DataAPI.MktEqudGet(tradeDate=last_day,secID=context.get_universe() ,field=u\"secID,openPrice,closePrice,highestPrice,lowestPrice,chgPct\",pandas=\"1\")\n        df_price1 = DataAPI.MktEqudGet(tradeDate=last_day1,secID=context.get_universe(),field=u\"secID,openPrice,closePrice,highestPrice,lowestPrice,chgPct\",pandas=\"1\")\n        df_price=df_price.set_index(['secID'])\n        df_price1=df_price1.set_index(['secID'])\n        \n        uplimit=DataAPI.MktLimitGet(secID=context.get_universe(),tradeDate=last_day,field=u\"secID,upLimitReachedTimes,limitUpPrice\",pandas=\"1\")\n        uplimit1=DataAPI.MktLimitGet(secID=context.get_universe(),tradeDate=last_day1,field=u\"secID,upLimitReachedTimes,limitUpPrice\",pandas=\"1\")        \n        df_upLimit = uplimit[uplimit['upLimitReachedTimes'] >= 1]\n        df_upLimit1 = uplimit1[ uplimit1['upLimitReachedTimes'] >= 1]\n        df_upLimit = df_upLimit.set_index(['secID'])\n        df_upLimit1 = df_upLimit1.set_index(['secID'])\n        prelist=[]\n        for stk in df_upLimit.index:\n            if df_price.ix[stk]['closePrice']==df_upLimit.ix[stk]['limitUpPrice']:\n                prelist.append(stk)\n\n        prelist1=[]\n        for stk in df_upLimit1.index:\n            if df_price1.ix[stk]['closePrice'] == df_upLimit1.ix[stk]['limitUpPrice']:\n                prelist1.append(stk)               \n        list0=list(set(prelist) & set(prelist1))\n        list_chu=list(set(list0+list_chu))\n    #\u7b5b\u51fa\u67092\u4e2a\u6da8\u505c\u7684\u80a1\u7968\u5217\u8868\n    \n    buy_list=[]\n    for stk in list_chu:\n        DATA = context.history(stk, ['closePrice','highPrice','lowPrice'], 240, freq='15m', rtype='frame', style='sat')\n        DATA = DATA[stk]\n        DATA['mean']=pd.rolling_mean(DATA['closePrice'],100)\n        DATA['std']=pd.rolling_std(DATA['closePrice'],26)\n        DATA['up']=DATA['mean']+3.3*DATA['std']\n        DATA['down']=DATA['mean']-3.3*DATA['std']\n    #print DATA\n        if context.current_price(stk)> DATA['down'][-1] and  DATA['lowPrice'][-2]<DATA['down'][-2]:\n            buy_list.append(stk)\n    \n    print buy_list\n    if len(buy_list)>0:\n        percent=min(1.0/len(buy_list),0.3)\n    for stock in buy_list:\n        if stock not in account.security_position.keys():\n            account.order_pct(stock,percent)\n     \n    for stock in  account.security_position.keys():\n        if context.current_price<=account.security_cost[stock]*0.97 or context.current_price>=account.security_cost[stock]*1.1 :\n            account.order_to(stock,0)\n        \n    ", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "Warning: refresh_rate\u7684\u503c\u4ec5\u4f5c\u7528\u4e8e\u5206\u949f\u7ebf\u3002\u82e5\u60f3\u5bf9\u65e5\u7ebf\u8fdb\u884c\u63a7\u5236\uff0c\u8bf7\u4f7f\u7528\u5982\u4e0b\u5b9a\u4e49: refresh_rate = (\u65e5\u7ebfrefresh_rate\uff0c \u5206\u949f\u7ebfrefresh_rate)"}, {"output_type": "stream", "stream": "stdout", "text": "\n"}, {"output_type": "stream", "stream": "stdout", "text": "[]\n[]"}, {"output_type": "stream", "stream": "stdout", "text": "\n[]"}], "trading_days": [1512950400000, 1513036800000], "trusted": true}], "metadata": {}}]}