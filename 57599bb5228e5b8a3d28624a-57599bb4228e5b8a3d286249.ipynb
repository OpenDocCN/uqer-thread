unt): # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    golden_bucket_on=[]\n    golden_bucket_under=[]\n    golden_bucket=[]\n    dead_bucket_on=[]\n    dead_bucket_under=[]\n    dead_bucket=[]\n    \n    for stk in account.universe:\n        k_c,d_c,j_c = KDJ(stk,start,account.current_date)\n        k_p,d_p,j_p = KDJ(stk,start,account.previous_date)\n\n        if  k_p < d_p and k_c > d_c and k_c > sellThres:\n             golden_bucket.append(stk)  \n#        if  slow_k_tmp[-2] > slow_d_temp[-2] and slow_k_tmp[-1] < slow_d_temp[-1] and slow_k_tmp[-1] < sellThres:\n#             dead_bucket.append(stk) \n\n    for stk in account.valid_secpos:\n        if stk in dead_bucket:\n                order_pct(stk,-1)  \n    for stk in golden_bucket:\n        order_pct(stk,0.01)\n    \n\ndef KDJ(ticker, dateb,datee):\n    df = DataAPI.MktIdxdGet(tradeDate=u\"\", indexID=u\"\", ticker=ticker, beginDate=dateb, endDate=datee, field=[\"tradeDate\", \"highestIndex\", \"lowestIndex\", \"closeIndex\"], pandas=\"1\")\n    hight = np.array(df[\"highestIndex\"])\n    low = np.array(df[\"lowestIndex\"])\n    close = np.array(df[\"closeIndex\"])\n    #matype: 0=SMA, 1=EMA, 2=WMA, 3=DEMA, 4=TEMA, 5=TRIMA, 6=KAMA, 7=MAMA, 8=T3 (Default=SMA)\n    K, D = talib.STOCH(hight, low, close, fastk_period=9, slowk_matype=0, slowk_period=3, slowd_period=3)\n    J = 3 * K - 2 * D\n    return(K[-1], D[-1], J[-1]) \n\n\n\n\n", "language": "python", "metadata": {}, "outputs": [{"ename": "AssertionError", "evalue": "high is not double", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mAssertionError\u001b[0m                            Traceback (most recent call last)", "\u001b[1;32m<mercury-input-4-7568d89c2242>\u001b[0m in \u001b[0;36m<module>\u001b[1;34m()\u001b[0m\n\u001b[0;32m    108\u001b[0m                                                           \u001b[0mslippage\u001