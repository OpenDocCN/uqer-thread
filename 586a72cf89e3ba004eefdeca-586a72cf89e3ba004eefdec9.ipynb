{"metadata": {"signature": "sha256:544b07097d622f6fda7eb7af340648187810184ac0b0114f88ed77f96fc4fd2f"}, "nbformat": 3, "nbformat_minor": 0, "worksheets": [{"cells": [{"cell_type": "markdown", "id": "F6D1160506A346AD94F87975C26D4511", "metadata": {}, "source": "# Notebook31"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "5DA97BA80A804F68B2E07EC08CC7573A", "input": "import pandas as pd\npd.set_option('precision', 2)\npd.set_option('display.width', 800)\nimport numpy as np\nimport datetime as dt\nimport math\nimport DataAPI\nDataAPI.settings.cache_enabled = True\nfrom collections import deque\n\n# \u7b2c\u4e00\u6b65\uff1a\u8bbe\u7f6e\u57fa\u672c\u53c2\u6570\nstart = '2016-12-01'\nend = '2016-12-31'\nbenchmark = 'HS300'\ncapital_base = 10000000\nfreq = 'd'\nrefresh_rate = 1\n\n# \u7b2c\u4e8c\u6b65\uff1a\u9009\u62e9\u56e0\u5b50\uff0c\u8bbe\u7f6e\u80a1\u7968\u6c60\nuniverse = StockScreener( Factor.TaxRatio.value_range(0.02,0.05) )\n\ndef dataPreprocess(account):\n    print account\n    account.data = account.get_daily_symbol_history(symbol = account, time_range = account.delay)\n    account.close = account.data['closePrice'].tolist()\n    account.high = account.data['highPrice'].tolist()\n    account.low = account.data['lowPrice'].tolist()\n    account.turnoverValue= account.dataturnoverValue['turnoverValue'].tolist()\n\n    pass\n\ndef initialize(account):\n    account.delay = max(4,3)\n    account.signal = {'long': False, 'short': False, 'stopLoss': False, 'stopWin': False, 'index':[]}\n    account.fenshijunjia = 0 #\u5206\u949f\u5747\u7ebf\n    account.xline = False    #\u5728\u5206\u949f\u5747\u7ebf\u4e0a\u4e0b\n\n    pass\n\ndef handle_data(account):\n    # \u751f\u6210\u4e70\u5165\u5217\u8868\n\n    buylist = [sec for sec in account.current_universe if sec in account.universe]\n    for stock in buylist:\n        dataPreprocess(stock)\n\n    v = account.reference_portfolio_value\n    d = len(buylist)\n\n    # \u5356\u51fa\u4e0d\u5728\u4e70\u5165\u5217\u8868\u4e2d\u7684\u80a1\u7968\uff0c\u4f30\u8ba1\u6301\u4ed3\u4ef7\u503c\n    for stock in account.security_position:\n        if stock not in buylist:\n            if stock in account.universe:\n                order_to(stock, 0)\n            else:\n                v -= account.security_position[stock] * account.reference_price[stock]\n\n    # \u83b7\u5f97\u8c03\u4ed3\u6570\u91cf\n    change = {}\n    for stock in buylist:\n        p = account.reference_price[stock]\n        if p:\n            change[stock] = int(v / d / p) - account.security_position.get(stock, 0)\n\n    # \u6309\u5148\u5356\u540e\u4e70\u7684\u987a\u5e8f\u53d1\u51fa\u6307\u4ee4\n    for stock in sorted(change, key=change.get):\n        if change[stock] <= -100 or change[stock] >= 100:\n            order(stock, change[stock])\n", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "600369.XSHG"}, {"output_type": "stream", "stream": "stdout", "text": "\n"}, {"ename": "AttributeError", "evalue": "'str' object has no attribute 'get_daily_symbol_history'", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mAttributeError\u001b[0m                            Traceback (most recent call last)", "\u001b[1;32m<mercury-input-24-5DA97BA80A804F68B2E07EC08CC7573A>\u001b[0m in \u001b[0;36m<module>\u001b[1;34m()\u001b[0m\n\u001b[0;32m     97\u001b[0m                                             \u001b[0mfreq\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0mfreq\u001b[0m\u001b[1;33m,\u001b[0m \u001b[0msecurity_base\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0msecurity_base\u001b[0m\u001b[1;33m,\u001b[0m \u001b[0msecurity_cost\u001b[0m\u001b[1;33m=\u001b[0m\u001b[0msecurity_cost\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\u001b[0m\u001b[0m\n\u001b[0;32m     98\u001b[0m                                             \u001b[0mmax_history_window\u001b[0m\u001b[1;33m=\u001b[0m\u001