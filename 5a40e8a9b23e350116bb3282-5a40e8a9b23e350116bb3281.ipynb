ialize(context):                   # \u521d\u59cb\u5316\u7b56\u7565\u8fd0\u884c\u73af\u5883\n    pass\n\ndef handle_data(context):                  # \u6838\u5fc3\u7b56\u7565\u903b\u8f91\n    global len_bizdates\n    tradingtime.append(str(context.current_date)[0:10]+' '+context.current_minute)\n    min_close.append(context.current_price(benchmark))\n    def test_data_len(tradingtime,min_close):\n        if 241*len_bizdates == len(tradingtime) and 241*len_bizdates == len(min_close):\n            pass\n        else:\n            print('lose data!')\n    if context.current_minute =='14:59':\n        tradingtime.append(str(context.current_date)[0:10]+' '+'15:00')\n        min_close.append(float(DataAPI.MktIdxdGet(tradeDate=str(context.current_date)[0:10],field='closeIndex',indexID=benchmark,pandas='1').values))\n    \n    if str(context.current_date)[0:10] == str(cal.adjustDate(datetime.date(int(end[0:4]),int(end[5:7]),int(end[8:]))\\\n                                            ,CAL.PyCAL.BizDayConvention.Preceding)) and context.current_minute=='14:59':\n        test_data_len(tradingtime,min_close)\n        min_k_line_data = pd.DataFrame(index=tradingtime, data=min_close)\n        min_k_line_data.to_excel('min_data_'+benchmark+'_'+start+'to'+end+'.xlsx','sheet1')", "language": "python", "metadata": {}, "outputs": [], "trading_days": "", "trusted": true}, {"cell_type": "markdown", "id": "CE68F658C0964E0B85301A1BD2082820", "metadata": {}, "source": "2\u3001\u83b7\u53d6\u4e2a\u80a1\u6570\u636e\uff0c\u81ea\u884c\u8bbe\u5b9astart\u3001end\u3001benchmark\u5e76\u6267\u884c\u7a0b\u5e8f\uff0c\u5373\u53ef\u4eceData\u4e2d\u4e0b\u8f7d\u6570\u636e"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "1024DEF9FE44442586DA0CB68B8B74C6", "input": "start = '2015-01-01'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2015-01-10'                         # \u56de\u6d4b\u7ed3\u675f\u65f6\u95f4\nuniverse = ['000001.XSHE']                 # \u8bc1\