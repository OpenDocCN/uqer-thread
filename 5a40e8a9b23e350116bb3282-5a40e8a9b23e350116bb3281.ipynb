[i+1]=str(context.current_date)[0:10]+' 15:00'\n        cdata=DataAPI.MktEqudAdjGet(tradeDate=context.current_date.strftime('%Y%m%d'),\\\n                    secID=universe[0],ticker=u\"\",beginDate=u\"\",endDate=u\"\",isOpen=\"\",field=u\"closePrice,turnoverVol,turnoverValue\",pandas=\"1\")\n        \n        min_close[i+1]=cdata[\"closePrice\"][0]\n        min_open[i+1]=np.nan\n        min_high[i+1]=np.nan\n        min_low[i+1]=np.nan\n        min_vol[i+1]=cdata[\"turnoverVol\"][0]-sum(min_vol[i-239:i+1])\n        min_Val[i+1]=cdata[\"turnoverValue\"][0]-sum(min_Val[i-239:i+1])\n        i+=1\n    i+=1\n    if str(context.current_date)[0:10]==str(cal.adjustDate(Date(int(end[0:4]),int(end[5:7]),int(end[8:])),\\\n                                                           BizDayConvention.Preceding)) and context.current_minute=='14:59':\n        k_line_data=pd.DataFrame({'TradeDatetime':pd.Series(tradingtime),'openPrice':pd.Series(min_open),'highPrice':pd.Series(min_high),\\\n                                  'lowPrice':pd.Series(min_low),'closePrice':pd.Series(min_close),'turnoverVol':pd.Series(min_vol),\\\n                                  'turnoverValue':pd.Series(min_Val)})\n        k_line_data.index = k_line_data['TradeDatetime']\n        k_line_data.drop('TradeDatetime',1,inplace=True)\n        k_line_data.to_excel(universe[0]+start+'to'+end+'.xlsx','sheet1')\n        \n    return \n\n", "language": "python", "metadata": {}, "outputs": [], "trading_days": "", "trusted": true}, {"cell_type": "code", "collapsed": false, "id": "73581F9D78944CC98B8AAD1563371A62", "input": "", "language": "python", "metadata": {}, "outputs": [], "trusted": true}], "metadata": {}}]}