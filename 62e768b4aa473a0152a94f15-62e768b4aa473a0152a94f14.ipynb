{"metadata": {"signature": "sha256:976dff5ad837de1fc43d28deba96056733c18c9a7a9e86613222fa3e4dcf3d61"}, "nbformat": 3, "nbformat_minor": 0, "original_thread": "62711efd7399a68ae76483a1", "worksheets": [{"cells": [{"cell_type": "markdown", "id": "CB7DF41FB85A4660A0A9FA85636677A3", "metadata": {}, "source": "# \u6bcf\u65e5\u6a21\u62df\u4ea4\u6613"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "23C651DD680C4C428D9FDBF9088D9806", "input": "import pandas as pd\nimport datetime\nimport lib.LewLib as LewLib\n\nstart = '2022-02-25'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2022-04-29'                         # \u56de\u6d4b\u7ed3\u675f\u65f6\u95f4\nuniverse = DynamicUniverse('HS300')        # \u8bc1\u5238\u6c60\uff0c\u652f\u6301\u80a1\u7968\u3001\u57fa\u91d1\u3001\u671f\u8d27\u3001\u6307\u6570\u56db\u79cd\u8d44\u4ea7\nbenchmark = 'HS300'                        # \u7b56\u7565\u53c2\u8003\u6807\u51c6\nfreq = 'd'                                 # \u7b56\u7565\u7c7b\u578b\uff0c'd'\u8868\u793a\u65e5\u95f4\u7b56\u7565\u4f7f\u7528\u65e5\u7ebf\u56de\u6d4b\uff0c'm'\u8868\u793a\u65e5\u5185\u7b56\u7565\u4f7f\u7528\u5206\u949f\u7ebf\u56de\u6d4b\nrefresh_rate = Monthly(-1)                 # \u8c03\u4ed3\u9891\u7387\uff0c\u8868\u793a\u6267\u884chandle_data\u7684\u65f6\u95f4\u95f4\u9694\uff0c\u82e5freq = 'd'\u65f6\u95f4\u95f4\u9694\u7684\u5355\u4f4d\u4e3a\u4ea4\u6613\u65e5\uff0c\u82e5freq = 'm'\u65f6\u95f4\u95f4\u9694\u4e3a\u5206\u949f\n\n# \u914d\u7f6e\u8d26\u6237\u4fe1\u606f\uff0c\u652f\u6301\u591a\u8d44\u4ea7\u591a\u8d26\u6237\ninitialCapital = 10000000\naccounts = {\n    'fantasy_account': AccountConfig(account_type='security', capital_base=initialCapital)\n    #'fantasy_account': AccountConfig(account_type='index', capital_base=initialCapital)\n}\n\ndef initialize(context):\n    pass\n  \n# \u6bcf\u4e2a\u5355\u4f4d\u65f6\u95f4(\u5982\u679c\u6309\u5929\u56de\u6d4b,\u5219\u6bcf\u5929\u8c03\u7528\u4e00\u6b21,\u5982\u679c\u6309\u5206\u949f,\u5219\u6bcf\u5206\u949f\u8c03\u7528\u4e00\u6b21)\u8c03\u7528\u4e00\u6b21\ndef handle_data(context):\n    print(context.now)\n    wROE = 0.7\n    wGuZhi = 0.3\n    flagQuantile=False\n    flagJQIndexCode=False\n    #dateEnd = LewLib.cutTradeDate(cuttingHour=16, cuttingMin=30)\n    #print(dateEnd)\n    \n    #dfAvailableIndexFundValFactor=LewLib.getAvailableIndexFundValFactor(context.now)\n    #dfAvailableIndexFund = LewLib.getAvailableIndexFundValFactorAsset(context.now)\n    \n    dfFactor = LewLib.getAvailableIndexFundValFactor(context.now, flagQuantile, flagJQIndexCode)\n    \n    # \u6dfb\u52a0\u4f30\u503c\u90e8\u5206\n    dfFactor = LewLib.addGuZhiFactor(dfFactor)\n    dfFactor.index.name = 'ETFID'\n    dfFactor = dfFactor.rename(columns={\"PEValue\": \"PETTM\"})\n    \n    if flagQuantile == True:\n        # \u989d\u5916\u8ba1\u7b97\u4e00\u4e2aQBGrade\n        dfFactor = dfFactor.sort_values(by=['QE'], ascending=False)\n        dfFactor['qeGrade'] = (dfFactor['QE'].rank(pct=True,ascending=False)*100).round(0)\n        dfFactor = dfFactor.sort_values(by=['QB'], ascending=False)\n        dfFactor['qbGrade'] = (dfFactor['QB'].rank(pct=True,ascending=False)*100).round(0)\n\n        dfFactor['GuZhiGrade'] = dfFactor[['peGrade', 'pbGrade', 'qeGrade', 'qbGrade']].sum(axis=1)\n    else:\n        dfFactor['GuZhiGrade'] = dfFactor[['peGrade', 'pbGrade']].sum(axis=1)\n        \n    dfFactor.index.name = 'IndexCode'\n    dfFactor = dfFactor.sort_values(by=['GuZhiGrade'], ascending=False)\n    \n    # \u6dfb\u52a0\u57fa\u91d1\u89c4\u6a21\n    dfAvailableIndexFund = LewLib.joinFundAsset(dfFactor, context.now, flagQuantile, flagJQIndexCode)\n    \n    # \u5bf9ROE\u56e0\u5b50\u6392\u5e8f\u6253\u5206\n    dfAvailableIndexFund = dfAvailableIndexFund.sort_values(by=['ROE'], ascending=False)\n    dfAvailableIndexFund['ROEGrade'] = (dfAvailableIndexFund['ROE'].rank(pct=True,ascending=True)*100).round(0)\n    \n    # \u589e\u52a0\u4e00\u5217\uff0c\u4f5c\u4e3aROE\u548c\u4f30\u503c\u56e0\u5b50\u7684\u7b97\u6cd5\n    dfAvailableIndexFund['Factor'] = dfAvailableIndexFund['ROEGrade']*wROE + dfAvailableIndexFund['GuZhiGrade']*wGuZhi\n    \n    dfAvailableIndexFund = dfAvailableIndexFund.sort_values(by=['Factor'], ascending=False)\n    \n    #df.to_csv('Datayes_Lew_dfAvailableIndexFundValFactor.csv', encoding='utf-8')\n    #df.to_csv('Datayes_Lew_dfAvailableIndexFundValFactor.csv', encoding='gbk')\n    dfAvailableIndexFund.to_excel(\"Lew_Datayes_dfAvailableIndexFundValROEFactor.xlsx\",encoding='utf-8')\n    \n    # \u6bcf\u4e2a\u6307\u6570\u9009\u62e9\u4e00\u4e2aETF\n    # \u9996\u5148\u53bb\u6389\u57fa\u91d1\u89c4\u6a21\u592a\u5c0f\u7684\uff0c\u6807\u51c6\uff1a1\u4ebf\n    dfQualifiedFund = dfAvailableIndexFund[dfAvailableIndexFund['netAsset'] > 100000000]\n    \n    # \u7136\u540e\u5bf9\u6bcf\u4e2a\u6307\u6570\n    dfIndexOrder = dfQualifiedFund[['idxID','idxShortName','Factor']]\n    dfIndexOrder = dfIndexOrder.drop_duplicates()\n    dfIndexOrder = dfIndexOrder.sort_values(by=['Factor'], ascending=False)\n    dfIndexOrder = dfIndexOrder.set_index('idxID')\n    \n    listTargetETF = []\n    for eachIndex in dfIndexOrder[:10].index:\n        df = dfQualifiedFund[dfQualifiedFund['idxID'] == eachIndex]\n        if len(df) == 1:\n            # \u5982\u679c\u53ea\u6709\u4e00\u4e2aETF\uff0c\u90a3\u5c31\u76f4\u63a5\u52a0\u5165\n            listTargetETF.append(df.index.tolist()[0])\n        elif len(df) > 1:\n            # \u5982\u679c\u6709\u591a\u4e2aETF\uff0c\u5219\u9009\u62e9\u89c4\u6a21\u6700\u5c0f\u7684\n            listTargetETF.append(df[df['netAsset'] == df['netAsset'].min()].index.tolist()[0])\n    # \u4ee5\u540e\u518d\u8003\u8651\u8d39\u7387\u5427\n    \n    # \u5c06\u56e0\u5b50\u503c\u4ece\u5c0f\u5230\u5927\u6392\u5e8f\uff0c\u5e76\u53d6\u524d10\u652f\u80a1\u7968\u4f5c\u4e3a\u76ee\u6807\u6301\u4ed3\n    target_position = listTargetETF\n    #target_position = dfIndexOrder[:10].index.tolist()\n    print(target_position)\n    \n    # \u83b7\u53d6\u5f53\u524d\u8d26\u6237\u4fe1\u606f\n    account = context.get_account('fantasy_account')   \n    #current_position = account.get_positions(exclude_halt=True)\n    current_position = account.get_positions()\n    print(current_position)\n    print('Selling:')\n    # \u5356\u51fa\u5f53\u524d\u6301\u6709\uff0c\u4f46\u76ee\u6807\u6301\u4ed3\u6ca1\u6709\u7684\u90e8\u5206\n    for stock in set(current_position).difference(target_position):\n        print(stock)\n        account.order_to(stock, 0)\n    \n    print('Buying:')\n    # \u6839\u636e\u76ee\u6807\u6301\u4ed3\u6743\u91cd\uff0c\u9010\u4e00\u59d4\u6258\u4e0b\u5355\n    for stock in target_position:\n        #result = account.order(stock, initialCapital/10)\n        if 'XSHG' in stock:\n            continue\n        else:\n            #orderID = account.order(stock, initialCapital/10, 'open')\n            orderID = account.order(stock, initialCapital/10)\n            print(account.get_order(orderID))\n    print account.get_orders()\n            \n'''\n    previous_date = context.previous_date.strftime('%Y-%m-%d')\n     \n    # \u83b7\u53d6\u56e0\u5b50PE\u7684\u7684\u5386\u53f2\u6570\u636e\u96c6\uff0c\u622a\u6b62\u5230\u524d\u4e00\u4e2a\u4ea4\u6613\u65e5\n    hist = context.history(symbol=context.get_universe(exclude_halt=True), attribute='PE', time_range=1, style='tas')[previous_date]\n    \n    # \u5c06\u56e0\u5b50\u503c\u4ece\u5c0f\u5230\u5927\u6392\u5e8f\uff0c\u5e76\u53d6\u524d100\u652f\u80a1\u7968\u4f5c\u4e3a\u76ee\u6807\u6301\u4ed3\n    signal = hist['PE'].order(ascending=True)\n'''\n", "language": "python", "metadata": {}, "outputs": [{"ename": "ImportError", "evalue": "No module named LewLib", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mImportError\u001b[0m                               Traceback (most recent call last)", "\u001b[1;32m<mercury-input-1-23C651DD680C4C428D9FDBF9088D9806>\u001b[0m in \u00