{"metadata": {"signature": "sha256:5d5ae6b38b3c0bcd49dbda34a085ec190b213616eebfdb0f8d0c1ad4ba097743"}, "nbformat": 3, "nbformat_minor": 0, "worksheets": [{"cells": [{"cell_type": "markdown", "id": "3DE487CD2E0F4416B3BE1EB85ED42CDA", "metadata": {}, "source": "# Notebook2"}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "5E7B2DFD77304B43817E6723BCE5F06C", "input": "# \u5229\u7528\u56de\u6d4b\u7b56\u7565\u8ba1\u7b97\u81ea\u5b9a\u4e49\u56e0\u5b50\uff0c\u5e76\u4fdd\u5b58\u5728Data\u73af\u5883\u7684CSV\u7535\u5b50\u8868\u4e2d\uff0c\u4ee5\u5907\u7b56\u7565\u56de\u6d4b\u65f6\u8c03\u7528\u3002\n# \u6b64\u7a0b\u5e8f\u4ec5\u6f14\u793a\u4e00\u4e2a\u7b80\u5355\u7684ATR\u56e0\u5b50\u7684\u8ba1\u7b97\u8fc7\u7a0b\uff0c\u4ee5\u53ca\u5904\u7406\u7684\u4e00\u4e9b\u6280\u5de7\u3002\n####################################################################\nimport numpy as np\nimport pandas as pd\nimport talib as ta #\u6280\u672f\u6307\u6807\u5e93\nfrom CAL.PyCAL import *\nfrom datetime import *\nfrom pandas import Series, DataFrame\nimport time\n\nstart = '2018-01-02' # \u8ba1\u7b97\u56e0\u5b50\u7684\u8d77\u59cb\u65e5\u671f\uff0c\u8981\u786e\u4fdd\u8fd9\u4e00\u5929\u662f\u4ea4\u6613\u65e5\uff0c\u6709\u6570\u636e\nend   = '2018-01-13' # \u7ed3\u675f\u8ba1\u7b97\u7684\u65e5\u671f\nuniverse = DynamicUniverse('A') # \u6307\u5b9aA\u80a1\u7684\u52a8\u6001\u8bc1\u5238\u6c60\nbenchmark = 'HS300' # \u7b56\u7565\u53c2\u8003\u57fa\u51c6\nfreq = 'd'          # \u65e5\u9891\u7387\u56de\u6d4b\nrefresh_rate = 1    # \u6267\u884chandle_data\u7684\u65f6\u95f4\u95f4\u9694\n# max_history_window = 60 #\u8bbe\u5b9acontext.history\u7684\u6570\u636e\u7a97\u5927\u5c0f\n\naccounts = {\n    'my_account': AccountConfig(account_type='security', capital_base=10000000)\n}\n\nglobal myFactor,ATR_periods #\u58f0\u660e\u4e3a\u5168\u5c40\u53d8\u91cf\nATR_periods = 30 #\u8ba1\u7b97ATR\u7684\u79fb\u52a8\u5e73\u5747\u5929\u6570\nprint '\u5f00\u59cb\u5904\u7406\u65f6\u95f4\uff1a ',time.strftime('%Y-%m-%d %H:%I:%M.%S', time.localtime(time.time()))\nprint\n\n####################################################################\n# \u7b2c\u4e00\u6b21\u8fd0\u884c\u65f6\u751f\u6210\u6570\u636e\u5e93\uff0c\u5982\u6709\u5219\u53d6\u5404\u80a1\u7968\u6700\u540e\u4ea4\u6613\u65e5\u7684ATR\u6570\u636e\uff08\u6709\u4e0d\u540c\u65e5\u671f\uff09\uff0c\n# \u8be5\u6570\u636e\u7528\u4e8e\u79fb\u52a8\u8ba1\u7b97\u4e0b\u4e00\u4e2a\u4ea4\u6613\u65e5\u7684ATR\u56e0\u5b50\u6570\u636e\n####################################################################\ndef initialize(context): # \u521d\u59cb\u5316\u7b56\u7565\u8fd0\u884c\u73af\u5883\n    global myFactor #\u58f0\u660e\u4e3a\u5168\u5c40\u53d8\u91cf\n    fields=['tradeDate','secID','highestPrice','lowestPrice']\n    context.my_factor='myATR.csv' #\u5b9a\u5236\u56e0\u5b50\u5e93,\u5b58\u50a8\u5728Data\u73af\u5883\u4e2d\n    # delete_files([context.my_factor,]) #\u6d4b\u8bd5\u65f6\u4f7f\u7528\n    if context.my_factor not in list_files():\n        myFactor = DataAPI.MktEqudAdjGet(tradeDate=start,isOpen=\"1\",field=fields,pandas=\"1\") #\u53d6\u7b2c\u4e00\u4e2a\u4ea4\u6613\u65e5\u7684\u6570\u636e\n        myFactor.set_index(['secID'],inplace=True)\n        myFactor['ATR'] = myFactor.highestPrice - myFactor.lowestPrice #\u751f\u6210\u7b2c\u4e00ATR\u6570\u636e\n        myFactor.drop(['highestPrice','lowestPrice'], axis=1, inplace=True) #\u5220\u9664\u4e0d\u9700\u8981\u7684\u5217\n        myFactor = myFactor[:4] #\u6d4b\u8bd5\u4ec5\u53d6\u524d4\u4e2a\u80a1\u7968\u6570\u636e\uff0c\u5b9e\u9645\u4f7f\u7528\u65f6\u5e94\u5220\u9664\n        myFactor.to_csv(context.my_factor, mode='a', header=True, encoding='GB18030') #\u4fdd\u5b58\u521d\u59cb\u7684ATR\u6570\u636e\u5e93\n    #end if\n    myFactor = pd.read_csv(context.my_factor,index_col=0,header=0) #\u8bfb\u5165ATR\u6570\u636e\u5e93\uff0c\u4ee5secID\u4e3a\u7d22\u5f15\uff0c\u7b2c\u4e00\u884c\u4e3a\u5217\u540d\n    context.last_date= myFactor[-1:].tradeDate.values[0] #\u4ee5\u6570\u636e\u5e93\u6700\u540e\u4e00\u884c\u7684\u65e5\u671f\u4e3a\u4e0a\u6b21\u5904\u7406\u7684\u6700\u540e\u65e5\u671f\uff0c\u907f\u514d\u91cd\u590d\u8ba1\u7b97\n    myFactor = myFactor.groupby(level='secID',group_keys=False) #\u4ee5\u73b0\u6709\u7684\u7d22\u5f15secID\u4e3a\u805a\u5408\u5206\u7ec4key\uff0c\u805a\u5408\u540e\u4e0d\u5305\u542bsecID\u5217\n    myFactor = myFactor.apply(lambda x: (x[-1:][['tradeDate','ATR']])) #\u53d6\u5404\u80a1\u7968\u7684\u6700\u540e1\u4e2a\u56e0\u5b50\uff0c\u4ee5\u9002\u5e94\u505c\u724c\u60c5\u51b5\n#end def\n  \n####################################################################\n# \u5229\u7528\u56de\u6d4b\u7b56\u7565\u6846\u67b6\uff0c\u9010\u65e5\u8ba1\u7b97ATR\u56e0\u5b50\uff0c\u5df2\u8ba1\u7b97\u7684\u65e5\u671f\u8df3\u8fc7\uff0c\u4ee5\u79ef\u7d2f\u56e0\u5b50\u6570\u636e\n####################################################################\ndef handle_data(context):\n    #account = context.get_account('my_account') #\u8bbe\u5b9a\u8d26\u6237\n    current_date = context.current_date.strftime('%Y-%m-%d') #\u56de\u6d4b\u65e5\u671f\n    if current_date <= context.last_date:\n        print current_date,'\u907f\u514d\u91cd\u590d\u5904\u7406\u6570\u636e'\n        return #\u907f\u514d\u91cd\u590d\u5904\u7406\u6570\u636e\n    #end if\n    factor = get_factor_ATR(context,current_date) #\u8ba1\u7b97\u5404\u80a1\u7968\u4e00\u5929\u7684\u56e0\u5b50\n    print\n    print current_date, len(factor) \n    print factor #\u663e\u793a\u5f53\u65e5\u7684\u56e0\u5b50\u6570\u636e\n    factor.to_csv(context.my_factor,index=True, mode='a', header=False, encoding='GB18030') #\u6bcf\u5929\u4ee5\u8ffd\u52a0\u7684\u65b9\u5f0f\u5199\u5165\u6570\u636e\u5e93\n#end def\n\n####################################################################\n# \u53d6\u6307\u5b9a\u65e5\u671f\u7684\u80a1\u7968\u6570\u636e\uff0c\u5408\u5e76\u524d\u4e00\u4ea4\u6613\u65e5\u7684ATR\u6570\u636e\uff0c\u7136\u540e\u6279\u91cf\u8ba1\u7b97\u5f53\u65e5\u7684ATR\n####################################################################\ndef get_factor_ATR(context,current_date):\n    global myFactor,ATR_periods #\u58f0\u660e\u4e3a\u5168\u5c40\u53d8\u91cf\n    fields = ['tradeDate','secID','openPrice','highestPrice','lowestPrice','closePrice','preClosePrice'] \n    gp = DataAPI.MktEqudAdjGet(isOpen=\"1\",beginDate=current_date,endDate=current_date,field=fields,pandas=\"1\")\n    gp.set_index('secID',inplace=True)\n    gp['HL'] = gp['highestPrice']-gp['lowestPrice']\n    gp['HC'] = abs(gp['highestPrice']-gp['preClosePrice'])\n    gp['CL'] = abs(gp['preClosePrice']-gp['lowestPrice'])\n    gp['ATR0'] = gp[['HL','HC','CL']].max(axis=1) #\u751f\u6210\u5f53\u65e5\u7684ATR\u6570\u636e\n    gp = gp.merge(myFactor[['ATR']],left_index=True,right_index=True) #\u5c06\u56e0\u5b50\u8868\u5408\u5230\u5f53\u65e5\u884c\u60c5\u8868\u4e2d\n    gp['ATR'] = (gp['ATR']*(ATR_periods-1.0)+gp['ATR0']*2.0)/(ATR_periods+1.0) #\u8ba1\u7b97\u79fb\u52a8\u5e73\u5747\u7684ATR\u6570\u636e\n    gp = gp[['tradeDate','ATR']] #\u53ea\u8981secID\uff08\u7d22\u5f15\uff09\u548ctradeDate\u3001ATR\u5217\n    gp = gp[:4] #\u6d4b\u8bd5\u4ec5\u53d6\u524d4\u4e2a\u80a1\u7968\u6570\u636e\uff0c\u5b9e\u9645\u4f7f\u7528\u65f6\u5e94\u5220\u9664\n    return gp\n#end def\n", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "\u5f00\u59cb\u5904\u7406\u65f6\u95f4\uff1a "}, {"output_type": "stream", "stream": "stdout", "text": " 2018-03-06 22:10:24.46\n\n"}, {"output_type": "stream", "stream": "stdout", "text": "2018-01-02 \u907f\u514d\u91cd\u590d\u5904\u7406\u6570\u636e\n"}, {"output_type": "stream", "stream": "stdout", "text": "2018-01-03 \u907f\u514d\u91cd\u590d\u5904\u7406\u6570\u636e\n2018-01-04 \u907f\u514d\u91cd\u590d\u5904\u7406\u6570\u636e\n2018-01-05 \u907f\u514d\u91cd\u590d\u5904\u7406\u6570\u636e\n\n"}, {"output_type": "stream", "stream": "stdout", "text": "2018-01-08 4\n              tradeDate       ATR\nsecID                            \n000001.XSHE  2018-01-08  0.574287\n0