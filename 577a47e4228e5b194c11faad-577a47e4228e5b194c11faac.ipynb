rice',1)).T\n    hist_price.sort(columns=0,inplace=True,ascending=True)\n    newUniverse=hist_price.index.values\n    cagnwei = round(account.cash /account.referencePortfolioValue,4)    #\u73b0\u6709\u7684\u7a7a\u95f2\u4ed3\n    lenbuy = 1\n    for stock in account.avail_secpos:\n        if (stock in account.referencePrice) and (account.referencePrice[stock]>account.price_flag[stock]*(1+sell_return)):\n            order_to(stock,0)\n            account.piece-=1\n            log.info(\"Sell\"+stock)\n    for stock in newUniverse: \n        if account.piece < position_piece:\n            if (stock in account.universe) and (not stock in account.valid_secpos):             \n                lenbuy +=1 \n  \n    cangweibl=cangwei/lenbuy\n\n    log.info(\"\u76f4\u63a5\u663e\u793a1\"+stock+\"\u6bd4\u4f8b\"+round(cangweibl,4))\n    for stock in newUniverse: \n        log.info(\"\u76f4\u63a5\u663e\u793a2\"+stock+\"\u6bd4\u4f8b\"+round(cangweibl,4))\n        if account.piece < position_piece:\n            if (stock in account.universe) and (not stock in account.valid_secpos):\n                try:\n                    order_pct_to(stock,cangwei/lenbuy)  # \u6da8\u505c\u5c31\u7b97\u4e86\uff0c\u81ea\u52a8\u7a7a\u51fa\u73b0\u91d1 \n                    log.info(\"Buy\"+stock+\"\u6bd4\u4f8b\"+round(cangweibl,4))\n                except:\n                    print(\"\u4e0d\u6210\u529f\"+stock+\"\u6bd4\u4f8b\")\n                    continue\n                account.price_flag[stock]=account.referencePrice[stock]\n    #return\n   \n    log.info(\"\u603b\u4ed3\u4f4d\u4e3a \"+str(round(1-account.cash /account.referencePortfolioValue,2)*100)+\"%\"+\"lenbuy \"+str(lenbuy))\n", "language": "python", "metadata": {}, "outputs": [{"ename": "NameError", "evalue": "global name 'cangwei' is not defined", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mNameError\u001b[0m                           