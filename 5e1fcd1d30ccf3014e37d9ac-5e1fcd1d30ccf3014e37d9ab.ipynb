u3002\ngetStockTradePoint(\"MA\",5,10)\ngetStockTradePoint(\"MA\",10,20)\n\u2026\u2026\n\n2\u3001\u8fd4\u56de\u503c\uff1a\n\uff081\uff09buy\uff1a\u4e70\u70b9\n\uff082\uff09sell\uff1a\u5356\u70b9\n'''\ndef getStockTradePoint(method='kdj',date=start,*args):\n    dtemp=DataAPI.TradeCalGet(exchangeCD=u\"XSHG\",beginDate=start,field=u\"\",pandas=\"1\")\n    #\u786e\u4fdd\u56de\u6d4b\u5f00\u59cb\u524d\u4e24\u4e2a\u4ea4\u6613\u65e5\u4e5f\u6709\u6570\u636e\n    twoDaysBefore=DataAPI.TradeCalGet(exchangeCD=u\"XSHG\",beginDate=dtemp['prevTradeDate'][0],endDate=dtemp['prevTradeDate'][0],field=u\"\",pandas=\"1\")['prevTradeDate'][0]\n    #kdj\n    if (method=='kdj'):\n        stockInfo=DataAPI.MktStockFactorsDateRangeGet(secID=stockID,beginDate=twoDaysBefore,endDate=end,field=u\"\",pandas=\"1\")[['KDJ_K','KDJ_D','tradeDate']]\n        if ((stockInfo.shift(2).loc[stockInfo['tradeDate']==date]['KDJ_K'].values[0]<stockInfo.shift(2).loc[stockInfo['tradeDate']==date]['KDJ_D'].values[0])&(stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['KDJ_K'].values[0]>stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['KDJ_D'].values[0])):\n            return 'buy'\n        if((stockInfo.shift(2).loc[stockInfo['tradeDate']==date]['KDJ_K'].values[0]>stockInfo.shift(2).loc[stockInfo['tradeDate']==date]['KDJ_D'].values[0])&(stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['KDJ_K'].values[0]<stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['KDJ_D'].values[0])):\n            return 'sell'\n    #MA    \n    if (method=='MA'):\n        if(len(args)==2):\n            #\u5982\u679c\u63d0\u4f9b\u7684\u5747\u503c\u4e0d\u57285,10,20,60,120\u65e5\uff0c\u5219\u62a5\u9519\n            if(MA.get(args[0])==None or MA.get(args[1])==None or args[0]==args[1]):\n                print(\"\u4e0d\u652f\u6301\u6b64\u7c7b\u5747\u7ebf\")\n                return\n            #\u5982\u679c\u7b2c\u4e00\u4e2a\u53c2\u6570\u66f4\u5927\uff0c\u8981\u4ea4\u6362\u4f4d\u7f6e\n            if(args[0]>args[1]):\n                temp=args[0]\n                args[0]=args[1]\n                args[1]=temp\n            stockInfo=DataAPI.MktStockFactorsDateRangeGet(secID=stockID,beginDate=twoDaysBefore,endDate=end,field=u\"\",pandas=\"1\")[[MA[args[0]],MA[args[1]],'tradeDate']]\n            if ((stockInfo.shift(2).loc[stockInfo['tradeDate']==date][MA[args[0]]].values[0]<stockInfo.shift(2).loc[stockInfo['tradeDate']==date][MA[args[1]]].values[0]) and (stockInfo.shift(1).loc[stockInfo['tradeDate']==date][MA[args[0]]].values[0]>stockInfo.shift(1).loc[stockInfo['tradeDate']==date][MA[args[1]]].values[0])):\n                return 'buy'\n            if((stockInfo.shift(2).loc[stockInfo['tradeDate']==date][MA[args[0]]].values[0]>stockInfo.shift(2).loc[stockInfo['tradeDate']==date][MA[args[1]]].values[0]) and (stockInfo.shift(1).loc[stockInfo['tradeDate']==date][MA[args[0]]].values[0]<stockInfo.shift(1).loc[stockInfo['tradeDate']==date][MA[args[1]]].values[0])):\n                return 'sell'\n    #MACD\n    if(method=='MACD'):\n        stockInfo=DataAPI.MktStockFactorsDateRangeGet(secID=stockID,beginDate=twoDaysBefore,endDate=end,field=u\"\",pandas=\"1\")[['MACD','tradeDate']]\n        if ((stockInfo.shift(2).loc[stockInfo['tradeDate']==date]['MACD'].values[0]<0) and (stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['MACD'].values[0]>0)):\n            return 'buy'\n        if ((stockInfo.shift(2).loc[stockInfo['tradeDate']==date]['MACD'].values[0]>0) and (stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['MACD'].values[0]<0)):\n            return 'sell'\n    #BOLL\n    if(method=='BOLL'):\n        stockInfo=DataAPI.MktStockFactorsDateRangeGet(secID=stockID,beginDate=twoDaysBefore,endDate=end,field=u\"\",pandas=\"1\")[['BollUp','BollDown','tradeDate']]\n        #\u53d6\u4ea4\u6613\u65e5\u524d1\u65e5\u6536\u76d8\u4ef7\u683c\u4e0e\u524d1\u65e5\u5e03\u6797\u7ebf\u4f5c\u6bd4\u8f83\u3002\n        closePrice=DataAPI.MktEqudGet(secID=stockID,ticker=u\"\",beginDate=twoDaysBefore,endDate=end,isOpen=\"\",field=u\"\",pandas=\"1\")[['closePrice','tradeDate']]\n    \n        #\u4e0b\u8e29\u5e03\u6797\u7ebf\uff0c\u4e70\u5165\n        if (closePrice.shift(1).loc[stockInfo['tradeDate']==date]['closePrice'].values[0]<stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['BollDown'].values[0]):\n            return 'buy'\n        #\u4e0a\u89e6\u5e03\u6797\u7ebf\uff0c\u5356\u51fa\n        if (closePrice.shift(1).loc[stockInfo['tradeDate']==date]['closePrice'].values[0]>stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['BollUp'].values[0]):\n            return 'sell'\n        \n    #RSI\n    if(method=='RSI'):\n        stockInfo=DataAPI.MktStockFactorsDateRangeGet(secID=stockID,beginDate=twoDaysBefore,endDate=end,field=u\"\",pandas=\"1\")[['RSI','tradeDate']]\n        if (stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['RSI'].values[0]<20):\n            return 'buy'\n        if (stockInfo.shift(1).loc[stockInfo['tradeDate']==date]['RSI'].values[0]>80):\n            return 'sell'\ndef initialize(context):\n    pass\n\ndef handle_data(context):  \n    account = context.get_account('fantasy_account')\n    current_date=context.current_date.strftime('%Y-%m-%d');\n    position= account.get_positions()\n    if (getStockTradePoint(method,current_date,5,10)=='buy'):\n        if (len(position.keys())==0):#\u6ca1\u6709\u6301\u4ed3\u7684\u65f6\u5019\u518d\u4e70\u5165\n            account.order_pct_to(stockID, 1)\n    if (getStockTradePoint(method,current_date,5,10)=='sell'):\n        if (len(position.keys())>0):#\u6709\u6301\u4ed3\u7684\u65f6\u5019\u518d\u5356\u51fa\n            account.order_to(stockID, 0)\n", "language": "python", "metadata": {}, "outputs": [{"metadata": {}, "output_type": "pyout", "prompt_number": 1, "text": "'{\"information\": 0.58436, \"benchmark_cumulative_return\": {\"1558656000000\": -0.0718462824, \"1562630400000\": -0.0501253133, \"1559174400000\": -0.0718462824, \"1573603200000\": 0.0217209691, \"1570752000000\": 0.0401002506, \"1560729600000\": -0