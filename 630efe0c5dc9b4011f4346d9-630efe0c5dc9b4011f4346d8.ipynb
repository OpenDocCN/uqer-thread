1d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    pass\n\ndef handle_data(context):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    current_date = context.current_date.strftime('%Y-%m-%d')\n    # if current_date < '2022-08-01':\n    #     return 0\n    buylist=[]\n    selist=[]\n    dt = Date.fromDateTime(context.current_date) \n    cal = Calendar('China.SSE')\n    lastTDay = cal.advanceDate(dt,'-1B',BizDayConvention.Preceding)\n    current_date=dt.strftime('%Y%m%d')\n    last_date=lastTDay.strftime('%Y%m%d')\n    \n    getData_current_date=DataAPI.MktStockFactorsOneDayGet(tradeDate=current_date,secID=context.universe,field=['secID','DEA','DIFF','NPParentCompanyGrowRate'],pandas=\"1\")\n    getData_current_date.set_index('secID',inplace=True)\n    # print getData_current_date['NPParentCompanyGrowRate']\n    getData_current_date=getData_current_date[getData_current_date.NPParentCompanyGrowRate>=1.0].dropna()\n    getData_current_date=getData_current_date.sort(columns='NPParentCompanyGrowRate',ascending=False)\n    getData_current_date=getData_current_date.head(50)\n   \n    getData_last_date=DataAPI.MktStockFactorsOneDayGet(tradeDate=last_date,secID=list(getData_current_date.index),field=['secID','DEA','DIFF','NPParentCompanyGrowRate'],pandas=\"1\")\n    getData_last_date.set_index('secID',inplace=True)\n    for stock in list(getData_current_date.index):\n        if getData_current_date['DIFF'][stock]:\n            if((getData_current_date['DIFF'][stock]>getData_current_date['DEA'][stock])&(getData_last_date['DIFF'][stock]<=getData_last_date['DEA'][stock])):\n                if getData_last_date['DIFF'][stock]>0:\n                    buylist.append(stock)\n            \n    for stock in list(getData_current_date.index):\n        if getData_current_date['DIFF'][stock]:\n            if((getData_current_date['DIFF'][stock]<=getData_current_date['DEA'][stock])&(getData_last_date['DIFF'][stock]>getData_last_date['DEA'][stock])):\n                selist.append(stock)\n    \n    \n    for stock in selist:  #\u80a1\u7968\u5356\u51fa\n        if(stock in context.security_position):\n            context.order_to(stock,0)\n        else:\n            pass\n        \n    if len(buylist)>0:\n        purchase = context.cash/len(buylist)\n    for stock in buylist:\n        if(stock in context.security_position):\n            pass\n        else:   #\u80a1\u7968\u4e70\u5165\n            if context.cash!=0:\n                context.order(stock,purchase/context.reference_price[stock])\n                \n            \n\n            \n    \n    \n\n", "language": "python", "metadata": {}, "outputs": [{"metadata": {}, "output_type": "pyout", "prompt_number": 16, "text": "'{\"information\": 3.37525, \"benchmark_cumulative_return\": {\"1653955200000\": -0.1718201538, \"1656892800000\": -0.0899411791, \"1648512000000\": -0.1631919596, \"1650844800000\": -0.2278088176, \"1652227200000\": -0.1951168751, \"1657843200000\": -0.1400392153, \"1650499200000\": -0.1911886497, \"1657584000000\": -0.1268638748, \"1647993600000\": -0.1343736312, \"1653609600000\": -0.1900816888, \"1645574400000\": -0.0642313406, \"1661731200000\": -0.1722243945, \"1658275200000\": -0.1328989856, \"1655856000000\": -0.1355668002, \"1652832000000\": -0.1919827394, \"1656460800000\": -0.1050560896, \"1658188800000\": -0.1358264162, \"1648425600000\": -0.1602929479, \"1659916800000\": -0.1603722334, \"1641772800000\": -0.0194981622, \"1648684800000\": -0.1452879077, \"1650585600000\": -0.1876626408, \"1648080000000\": -0.1394767476, \"1647216000000\": -0.1549712043, \"1656633600000\": -0.095874415, \"1652140800000\": -0.2065643299, \"1655251200000\": -0.1340294467, \"1655942400000\": -0.1207388923, \"1655424000000\": -0.1277897158, \"1644796800000\": -0.0786753301, \"1653264000000\": -0.1794178185, \"1658448000000\": -0.1421227015, \"1652745600000\": -0.1891516781, \"1657238400000\": -0.1035533691, \"1655164800000\": -0.1453456766