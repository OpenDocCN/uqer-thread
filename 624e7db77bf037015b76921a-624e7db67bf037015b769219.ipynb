_date = context.current_date.strftime('%Y-%m-%d')\n    account = context.get_account('fantasy_account')  \n    current_position = account.get_positions(exclude_halt=True)  \n    #print today_date\n    \n    #\u5224\u65ad\u662f\u5426\u662f\u6708\u521d\u6570\u636e\n    data0 = DataAPI.WorkingCalGet(beginDate=today_date,endDate=today_date,isWork=u\"\",prevWorkDate=u\"\",field=u\"\",pandas=\"1\")\n    #\u5224\u65ad\u662f\u5426\u662f\u6708\u672b\u6570\u636e\n    monthend =DataAPI.TradeCalGet(exchangeCD=u\"XSHG\",beginDate=today_date,endDate=today_date,isOpen=u\"1\",field=u\"\",pandas=\"1\")\n\n    #\u5224\u65ad\u662f\u4e0d\u662f\u6708\u521d\uff0c\u82e5\u662f\u6708\u521d\uff0c\u5219\u9009\u80a1\u5e76\u4e70\u5165\n    if today_date == data0.ix[0,'monthStartDate']:\n        previous_date = context.previous_date.strftime('%Y-%m-%d')\n        # \u83b7\u53d6\u56e0\u5b50PE\u7684\u7684\u5386\u53f2\u6570\u636e\u96c6\uff0c\u622a\u6b62\u5230\u524d\u4e00\u4e2a\u4ea4\u6613\u65e5\n        hist = context.history(symbol=context.get_universe(exclude_halt=True), attribute='PE', time_range=1, style='tas')[previous_date]\n        # \u5c06\u56e0\u5b50\u503c\u4ece\u5c0f\u5230\u5927\u6392\u5e8f\uff0c\u5e76\u53d6\u524d5\u652f\u80a1\u7968\u4f5c\u4e3a\u76ee\u6807\u6301\u4ed3\n        signal = hist['PE'].order(ascending=True)\n        target_position = signal[:5].index\n        \n        #print target_position\n        for stock in target_position: #\u4ee5\u5f53\u5929\u6536\u76d8\u4ef7\u4e0b\u5355\n            account.order(stock, 10000)\n    \n    #\u82e5\u662f\u6708\u672b\uff0c\u5219\u5356\u51fa\u80a1\u7968    \n    if monthend.ix[0,'isMonthEnd'] == 1:\n        for stock in current_position:\n            account.order_to(stock,0)  \n\n    \n    #\u82e5\u4e0d\u662f\u6708\u672b\u4e5f\u4e0d\u662f\u6708\u521d\uff0c\u5224\u65ad\u6b62\u635f\u6761\u4ef6\n    #print(current_position)\n    #print(current_position['600100.XSHG'])\n    \n    \n    if today_date != data0.ix[0,'monthStartDate'] and monthend.