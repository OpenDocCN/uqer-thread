70\nDMA_N1 = 10\nDMA_N2 = 50\nDMA_M = 10\nstk_num = 20      # \u6301\u4ed3\u80a1\u7968\u6570\u91cf\nlongest_history = 240    #1200\nmax_history_window = (10,240)\nTimelist= ['10:30','11:00','14:00','14:30'] \naDayTimes = 0  #\u8bb0\u5f55\u5f53\u524d\u65e5\u671f\u6267\u884c\u4e86\u51e0\u6b21\u3002\n\ndef initialize(account):\n    pass\n    \ndef handle_data(account):\n    global aDayTimes\n    minuteprices = pd.DataFrame()\n    minuteday = 0\n    if account.current_minute not in Timelist: return\n    try:\n        minuteday=float(read('minuteday.txt',encoding = 'utf-8'))\n        minuteprices = pd.read_csv('minuteprices.csv',index_col=0)\n    except:\n        minuteday = 0\n        write('minuteday.txt',str(minuteday), encoding = 'utf-8')\n        minuteprices.to_csv('minuteprices.csv',encoding = 'utf-8')\n        \n    list1 = aDayTimes % len(Timelist)\n    if list1 == 0:\n        list2 = len(Timelist) - 1\n    else:\n        list2 = list1 - 1\n    minuteslen = minutesdif(Timelist[list2],Timelist[list1])\n    all_close_prices = account.get_attribute_history('closePrice', minuteslen) \n    cur_close_prices = pd.DataFrame(all_close_prices)\n    minuteprices = pd.merge(minuteprices, cur_close_prices, how='outer')\n    minuteprices = minuteprices[minuteslen:][:]\n    minuteday = minuteday + 1./len(Timelist)\n    minuteprices.to_csv('minuteprices.csv',encoding = 'utf-8')\n    write('minuteday.txt',str(minuteday), encoding = 'utf-8')\n    aDayTimes = aDayTimes + 1\n        \n    all_close_prices = minuteprices.to_dict(outtype='list')\n    MACD60_bucket = []\n    DMA60_bucket = []\n    MACD30_bucket = []\n    DMA30_bucket = []\n    for stk in account.universe:\n        #-----60\u5206\u949f ---------------------------------------------------------------------------------------------\n        prices = all_close_prices[stk]\n        priceslen=len(prices)\n        prices = np.array([prices[i] for i in range(priceslen) if i%60==59])\n        if prices is None:\n            log.info( 'None')\n            continue\n        try: # talib\u8ba1\u7b97MACD\n            macd_tmp = talib.MACD(prices, fastperiod=MACD_short, slowperiod=MACD_long, signalperiod=MACD_win)\n            DIF = macd_tmp[0]\n            DEA = macd_tmp[1]\n            MACD = macd_tmp[2]\n        except:\n            log.info( 'MACD60 cal')\n            continue\n        if round(DIF[-1],3) > round(DEA[-1],3) and round(DIF[-2],3) < round(DEA[-2],3) \\\n        and (DIF[-1] > DIF[-2] or DIF[-1] > DIF[-3]):# and DEA[-1] > DEA[-2]:\n            MACD60_bucket.append(stk)\n        try: # talib\u8ba1\u7b97DMA\n            DMA_DIF = talib.MA(prices,DMA_N1,matype=0) - talib.MA(prices,DMA_N2,matype=0) \n            DMA_DIFMA = talib.MA(DMA_DIF,DMA_M,matype=0)\n        except:\n            log.info( 'DMA60 cal')\n            continue\n        if round(DMA_DIF[-1],3) > round(DMA_DIFMA[-1],3) and round(DMA_DIF[-2],3) < round(DMA_DIFMA[-2],3) \\\n        and (DMA_DIF[-1] > DMA_DIF[-2] or DMA_DIF[-1] > DMA_DIF[-3]) :#and DMA_DIFMA[-1] > DMA_DIFMA[-2]:\n            DMA60_bucket.append(stk)\n        #-----30\u5206\u949f ----------------------------------------------------------------------------------------\n        prices = all_close_prices[stk]\n        priceslen=len(prices)\n        prices = np.array([prices[i] for i in range(priceslen) if i%30==29])\n        if prices is None:\n            log.info( 'None')\n            continue\n        try: # talib\u8ba1\u7b97MACD\n            macd_tmp = talib.MACD(prices, fastperiod=MACD_short, slowperiod=MACD_long, signalperiod=MACD_win)\n            DIF = macd_tmp[0]\n            DEA = macd_tmp[1]\n            MACD = macd_tmp[2]\n        except:\n            log.info( 'MACD30 cal')\n            continue\n        if round(DIF[-1],3) > round(DEA[-1],3) and round(DIF[-2],3) < round(DEA[-2],3) \\\n        and (DIF[-1] > DIF[-2] or DIF[-1] > DIF[-3]):# and DEA[-1] > DEA[-2]:\n            MACD30_bucket.append(stk)\n        try: # talib\u8ba1\u7b97DMA\n            DMA_DIF = talib.MA(prices,DMA_N1,matype=0) - talib.MA(prices,DMA_N2,matype=0) \n            DMA_DIFMA = talib.MA(DMA_DIF,DMA_M,matype=0)\n        except:\n            log.info( 'DMA30 cal')\n            continue\n        if round(DMA_DIF[-1],3) > round(DMA_DIFMA[-1],3) and round(DMA_DIF[-2],3) < round(DMA_DIFMA[-2],3) \\\n        and (DMA_DIF[-1] > DMA_DIF[-2] or DMA_DIF[-1] > DMA_DIF[-3]):# and DMA_DIFMA[-1] > DMA_DIFMA[-2]:\n            DMA30_bucket.append(stk)\n        \n    strmail = str(account.current_minute)\n    strmail = strmail + '\\n MACD60: \\n' + ' '.join(MACD60_bucket)\n    strmail = strmail + '\\n MACD30: \\n' + ' '.join(MACD30_bucket)\n    strmail = strmail + '\\n DMA60: \\n' + ' '.join(DMA60_bucket)\n    strmail = strmail + '\\n DMA30: \\n' + ' '.join(DMA30_bucket)\n    log.info( strmail[0:100] )\n    \n    # send_mail_return = send_mail('ggd2005@qq.com','MACD/DMA mail from Uqer',strmail)\n    # if send_mail_return['code'] == 200 :\n    #     log.info( 'send mail success!' )\n\n    return\n\n#\u8ba1\u7b97\u4e24\u4e2alist\u65f6\u95f4\u4e4b\u95f4\u7684\u5206\u949f\u6570\u5dee\ndef minutesdif(time1,time2):\n    am1 = datetime.datetime.strptime('2015-05-01 09:30' ,\"%Y-%m-%d %H:%M\")\n    am2 = datetime.datetime.strptime('2015-05-01 11:30' ,\"%Y-%m-%d %H:%M\")\n    pm1 = datetime.datetime.strptime('2015-05-01 13:00' ,\"%Y-%m-%d %H:%M\")\n    pm2 = datetime.datetime.strptime('2015-05-01 15:00' ,\"%Y-%m-%d %H:%M\")\n    a = datetime.datetime.strptime('2015-05-01 ' +time1 ,\"%Y-%m-%d %H:%M\")\n    b = datetime.datetime.strptime('2015-05-01 ' +time2 ,\"%Y-%m-%d %H:%M\")\n    len= (b-a).seconds/60\n    if ((am1<=a and a<=am2 and am1<=b and b<=am2) or (pm1<=a and a<=pm2 and pm1<=b and b<=pm2)) and a<b:  #print '\u540c\u4e00\u65f6\u95f4\u65ad'\n        return len\n    elif (am1<=a and a<=am2 and pm1<=b and b<=pm2):  #print 'a\u5728\u4e0a\u5348\uff0cb\u5728\u4e0b\u5348'\n        return len-90\n    elif (am1<=b and b<=am2 and pm1<=a and a<=pm2):  #print 'b\u5728\u4e0a\u5348\uff0ca\u5728\u4e0b\u5348'\n        return len-1110\n    else:  #print '\u9694\u65e5'\n        return 240", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "stdout", "text": "\u5f00\u59cb\u52a0\u8f7d\u6570\u636e"}, {"output_type": "stream", "stream": "stdout", "text": "\n"}, {"output_type": "stream", "stream": "stdout", "text": "\u5f00\u59cb\u8fdb\u884c\u56de\u6d4b\n2017-05-10 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 10:30\n MACD60: \n002823.XSHE 002830.XSHE 002843.XSHE\n MACD30: \n002826.XSHE 603578.XSHG 603660.XSHG\n D\n2017-05-10 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 11:00\n MACD60: \n002830.XSHE 002843.XSHE\n MACD30: \n\n DMA60: \n002823.XSHE 600926.XSHG\n DMA30: \n002831.\n2017-05-10 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 14:00\n MACD60: \n601997.XSHG\n MACD30: \n300577.XSHE\n DMA60: \n\n DMA30: \n\n2017-05-10 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 14:30\n MACD60: \n601997.XSHG\n MACD30: \n300585.XSHE\n DMA60: \n002843.XSHE\n DMA30: \n601375.XSHG\n2017-05-10 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 14:50\n MACD60: \n\n MACD30: \n300585.XSHE\n DMA60: \n002843.XSHE\n DMA30: \n002836.XSHE 300585.XSHE 603987.\n"}, {"output_type": "stream", "stream": "stdout", "text": "2017-05-11 00:00:00 [INFO] 10:30\n MACD60: \n300569.XSHE 603663.XSHG\n MACD30: \n300623.XSHE 300647.XSHE 300650.XSHE 603501.XSHG 60\n2017-05-11 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 11:00\n MACD60: \n603663.XSHG\n MACD30: \n300567.XSHE 300582.XSHE 603179.XSHG 603228.XSHG 603229.XSHG 60\n2017-05-11 00:00:00"}, {"output_type": "stream", "stream": "stdout", "text": " [INFO] 14:00\n MACD60: \n\n MACD30: \n300562.XSHE 300567.XSHE 300649.XSHE 603032.XSHG 603078.XSHG 603556.XSHG\n \n2017-05-11 00:00:00"}, {"output_type": "stream", "stream":