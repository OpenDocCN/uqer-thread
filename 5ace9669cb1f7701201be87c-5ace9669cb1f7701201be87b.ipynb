{"metadata": {"signature": "sha256:db703d747776bdfa12715d281b434d8aff23ad0f5a2669a14b2a53b279c5fa04"}, "nbformat": 3, "nbformat_minor": 0, "worksheets": [{"cells": [{"cell_type": "markdown", "id": "FA03A661932B4547A068A44E91FEFE18", "metadata": {}, "source": "# NewBollingerTrader1.4.68_work"}, {"cell_type": "strategy", "collapsed": false, "folded": false, "has_detail": true, "id": "436612FE97AB409186FE1B59B17FD941", "input": "#from __future__ import division\n#\u65e5\u5fd7\n#\u7248\u672c1.3\n#=====\n#\u4e70\u5165:N\u65e5\u6700\u5c0f\u4ea4\u6613\u91cf\u3002\u6536\u76d8\u4ef7\u4e3aN\u65e5\u6ce2\u52a8\u533a\u95f4\u7684\u6700\u5c0f%P\n#\u4e70\u5165\u5bf9\u8c61\u8fc7\u591a\u65f6\uff0c\u6309\u7167\u6700\u8fd1N\u65e5\u6da8\u8dcc\u5e45\u5c0f\u5230\u5927\u6392\u5e8f\n#\u5356\u51fa\uff1a\n# 1\uff0c\u5f53\u6700\u5c0f\u5747\u7ebf\u56de\u843d\u65f6\u3002\u6700\u5927\u6536\u76ca\u56de\u843d%M, \u6b62\u76c8\u3002\n# 2\uff0c\u5f53\u6700\u5c0f\u5747\u7ebf\u56de\u843d\u65f6\u3002\u76c8\u5229\u5230\u8fbe%P\uff0c\u6b62\u76c8\u3002\n# 3\uff0c\u4e8f\u635f\u5927\u4e8e%L, \u6b62\u635f\u3002\n# 4\uff0c\u5e03\u6797\u7ebf4\u5468\u671f\u6700\u5927\n#\u5bf9\u8c61\uff1a\n#\u4ece\u9009\u80a1\u5b9d\u9009\n#=====\n#\u7248\u672c1.1\n#\u4fee\u6539\uff1a\u6301\u4ed3\u80a1\u7968\u6570\u4e0d\u80fd\u5927\u4e8estk_num\n#\u4f18\u5316\uff1a\u5b8c\u5584log\u663e\u793a\n#1.4.57.1\n#\u4f7f\u7528\u6587\u4ef6\u50a8\u5b58\u6628\u65e5\u6301\u4ed3\u503c\n#=====\n\n#========import========\nfrom __future__ import division\nimport numpy as np\nimport pandas as pd\nimport talib\nfrom talib import func\nimport matplotlib\nfrom CAL.PyCAL import *\nimport datetime  \n\n#========\u57fa\u672c\u53c2\u6570========\nstart = '2017-01-01'\nend = '2018-01-06'\nbenchmark = 'HS300'\nrefresh_rate = 1\nmax_history_window = 100\n\naccounts = {\n    'fantasy_account': AccountConfig(account_type='security', capital_base=200000)\n}\n\n#=====\u6301\u4ed3\u53c2\u6570======\nstk_num = 12  #\u6301\u80a1\u6570\u91cf\n\n#=====\u6b62\u76c8\u6b62\u635f\u53c2\u6570====\nSTOPLOSS = 0.13  #\u4e8f\u635f\u5927\u4e8e%L\nTAKEPROFIT = 0.40 #\u76c8\u5229\u5230\u8fbe%P\nDAYNAMIC_TP = 0.08 #\u6700\u5927\u6536\u76ca\u56de\u843d%M\n\n#=====\u5e03\u6797\u7ebf\u53c2\u6570======\nBOLL_MA = 20\nBOLL_DEV = 2\nBOLL_DEV2 = 3\nBOLL_CHECK_DAYS = 30\n\n#=====\u793e\u4ea4\u6307\u6570======\nSOCIAL_BUY_RATE = 0.25\n\n#=====Vol\u53c2\u6570======\nVOL_DAYS = 15\nNDAY_RANGE = VOL_DAYS\n\n#=====\u4ef7\u683c\u53c2\u6570======\nNDAY_PERCENT_P = 0.3\n\n#=====MA\u53c2\u6570====\nMA1 = 5\nMA2 = 10\nMA3 = 20\nMA4 = 30\n\n#=====\u6392\u5e8f\u53c2\u6570====\nSORT_DAYS = VOL_DAYS\n\n#========\u80a1\u7968\u6c60========\ndef get_universe():\n    all_stock = pd.read_csv('all_stock.csv',encoding='GB18030')\n    all_stock = all_stock[all_stock.Trade==1]\n    all_stock = all_stock.secID.tolist()\n    return all_stock\n    \nuniverse = get_universe()\n#['600004.XSHG','600005.XSHG','600157.XSHG','002221.XSHE','300175.XSHE']\n\n#=====\u5168\u5c40\u53d8\u91cf======\nMAX_HISTORY_DAYS = MA4*2\nTURNOVER_VALUE_CHECK_DAYS = MA4\ncal = Calendar('China.SSE')\n    \n# \u628a\u5168A\u80a1\u7684\u4e0a\u5e02\u65e5\u671f\u90fd\u62ff\u51fa\u6765\ndef get_listinfo():\n    \"\"\"\u83b7\u53d6\u5168A\u80a1\u4e0a\u5e02\u65e5\u671f\"\"\"\n\n    listinfo = {}\n    wk_universe = set_universe('A')\n    for i in range(0, len(wk_universe), 300):\n        batch = wk_universe[i:min(len(wk_universe), i+300)]\n        batch = ','.join([s[:6] for s in batch])\n        df = DataAPI.SecIDGet(ticker=batch, assetClass='E', field='secID,listDate')\n        for j in range(len(df)):\n            listinfo[df.at[j, 'secID']] = datetime.datetime.strptime(df.at[j, 'listDate'], '%Y-%m-%d')\n    return listinfo\n\nLISTINFO = get_listinfo()\n\n#\u83b7\u5f97\u5386\u53f2\u6570\u636e\uff0c\u53bb\u9664\u505c\u724c\u671f\u95f4\u7684\u6570\u636e\ndef GetHistoryDate(stock, getDays, account,context,max_no_data_days=30):\n    \"\"\"\u6e05\u6d17\u5386\u53f2\u6570\u636e\"\"\"\n    wk_universe = context.get_universe(exclude_halt=True)\n    if stock not in wk_universe: # \u5f53\u65e5\u505c\u724c\n        return [], False\n\n    newSize = 0\n    getSize = max_history_window\n\n    #\u8f6c\u6362\u65e5\u671f\u683c\u5f0f\n    strDays = '-{}B'.format(getSize)\n    wk_DT = cal.advanceDate(context.current_date, strDays, BizDayConvention.Following) \n    wk_DT = wk_DT.toDateTime()\n\n    #\u65e5\u5b50\u56de\u6eaf\u5230\u4e0a\u5e02\u4e4b\u524d\uff0c\u6570\u636e\u4e0d\u8db3\n    if stock not in LISTINFO:\n        return [], False\n    if LISTINFO[stock] > wk_DT:\n        return [], False \n\n    #\u53d6\u5f97\u5386\u53f2\u6570\u636e\uff0c\u8fd9\u4e0d\u662fDataframe\n    try:\n        symb_hist = context.history(symbol=stock, attribute=['openPrice','closePrice','highPrice','lowPrice','turnoverVol'] ,time_range=getSize )\n    except Exception,ex:\n        log.info(\"Exception when get history\")\n        log.info(ex)\n        return [], False\n\n    symb_hist = symb_hist[stock]\n\n    #print symb_hist\n\n    #\u53bb\u9664\u505c\u724c\u65e5\u671f\n    is_valid = (symb_hist['openPrice'] > 0) & (~np.isnan(symb_hist['openPrice'])) &\\\n               (symb_hist['closePrice'] > 0) & (~np.isnan(symb_hist['closePrice'])) &\\\n               (symb_hist['turnoverVol'] > 0) & (~np.isnan(symb_hist['turnoverVol']))\n\n\n    #print is_valid\n    # \u5148\u5224\u65ad\u662f\u5426\u53ef\u884c\u518d\u66f4\u65b0\u6570\u636e\n    newSize = sum(is_valid)\n    if newSize >= getDays:\n        for ind in symb_hist.index:\n            if is_valid.loc[ind] == False:\n                symb_hist = symb_hist.drop(ind)\n        wk_size = len(symb_hist)\n        symb_hist = symb_hist[wk_size-getDays:wk_size]\n        return symb_hist, True\n    else:\n        return [], False  \n            \n\n#\u83b7\u5f97\u5747\u7ebf\ndef get_MA(data,days):\n    \n    wk_closePrice = data['closePrice']\n    wk_MA = []\n    for index in range(len(wk_closePrice),days,-1):\n        MA = np.array(wk_closePrice[index-days:index]).mean()\n        wk_MA.insert(0,MA)\n            \n    return wk_MA \n\n#\u83b7\u5f97Vol\u5747\u7ebf\n#\u83b7\u5f97\u5747\u7ebf\ndef get_Vol_MA(data,days):\n    \n    wk_Vol = data['turnoverVol']\n    wk_Vol_MA = []\n    for index in range(len(wk_Vol),days,-1):\n        MA = np.array(wk_Vol[index-days:index]).mean()\n        wk_Vol_MA.insert(0,MA)\n            \n    return wk_Vol_MA \n\n#\u83b7\u5f97\u5e03\u6797\u7ebf\ndef get_BANDS(data):\n    \n    wk_closePrice = data['closePrice']\n    wk_closePrice = wk_closePrice.values\n    \n    #\u53d6\u5f97\u5e03\u6797\u7ebf\u7684\u503c\n    upper, middle, lower = func.BBANDS(wk_closePrice, timeperiod=BOLL_MA,nbdevup=BOLL_DEV, nbdevdn=BOLL_DEV,matype=talib.MA_Type.SMA) \n    \n    return upper, middle, lower\n\n#\u83b7\u5f97\u5e03\u6797\u7ebf2\ndef get_BANDS2(data):\n    \n    wk_closePrice = data['closePrice']\n    wk_closePrice = wk_closePrice.values\n    \n    #\u53d6\u5f97\u5e03\u6797\u7ebf\u7684\u503c\n    upper2, middle2, lower2 = func.BBANDS(wk_closePrice, timeperiod=BOLL_MA,nbdevup=BOLL_DEV2, nbdevdn=BOLL_DEV2,matype=talib.MA_Type.SMA) \n    \n    return upper2, middle2, lower2\n\n#\u83b7\u5f97N\u65e5\u6da8\u8dcc\u5e45\ndef get_UPDOWN_Percent(data):\n    \n    wk_closePrice = data['closePrice']\n    wk_closePrice = wk_closePrice[-1]\n    \n    wk_openPrice = data['openPrice']\n    wk_openPrice = wk_openPrice[-SORT_DAYS]\n    \n    wk_movePrice = wk_closePrice - wk_openPrice\n    wk_movePercent = wk_movePrice/wk_openPrice\n    \n    return wk_movePercent\n\n#\u83b7\u5f97N\u65e5\u6ce2\u52a8\u533a\u95f4\ndef get_NDay_Range(data):\n    \n    wk_highPrice = data['highPrice']\n    wk_lowPrice = data['lowPrice']\n    \n    highValue = wk_highPrice[-1]\n    lowValue = wk_lowPrice[-1]\n    for i in range(2,NDAY_RANGE+1):\n        if highValue < wk_highPrice[-i]:\n            highValue = wk_highPrice[-i]\n        if lowValue > wk_lowPrice[-i]:\n            lowValue = wk_lowPrice[-i]        \n            \n    return highValue,lowValue\n\n#N\u65e5\u6ce2\u52a8\u533a\u95f4\u6700\u5c0f%P\ndef determin_NDay_Min(data):\n \n    ret_bool = False\n    \n    highValue,lowValue = get_NDay_Range(data)\n    wk_closePrice = data['closePrice']\n    \n    rangeValue = highValue - lowValue\n    aboveLowValue = rangeValue*NDAY_PERCENT_P + lowValue\n    \n    if wk_closePrice[-1] < aboveLowValue:\n        ret_bool = True\n    \n    return ret_bool\n\ndef determin_Vol_Buy(data):\n    \n    ret_bool = False\n    \n    #\u83b7\u5f97\u6210\u4ea4\u91cf\u5747\u7ebf\n    wk_Vol_MA1 = get_Vol_MA(data,MA1)\n    wk_Vol_MA2 = get_Vol_MA(data,MA2)\n\n    #\u83b7\u5f97Vol\n    wk_Vol = data['turnoverVol']\n    \n    if (wk_Vol[-1] > wk_Vol_MA1[-1]) & (wk_Vol[-1] > wk_Vol_MA2[-1]):\n        ret_bool = True\n    else:\n        return False\n    \n    #N\u65e5\u5185\u6210\u4ea4\u91cf\u6700\u5c0f\n    #length = len(data['turnoverVol'])\n    #wk_turnoverValue = data['turnoverVol'][length-VOL_DAYS:length]\n    #wk_min = min(wk_turnoverValue) \n    #if wk_min == wk_turnoverValue[-1]:\n    #    ret_bool = True\n    \n    return ret_bool\n\n#1,4\u6761\u5747\u7ebf\u591a\u5934\u6392\u5217\uff0c\u65e2\u5747\u7ebf1>2>3>4\n#\u51b3\u5b9a\u5747\u7ebf\u4e70\u5165\u6761\u4ef6\ndef determin_MA_Buy(data):\n    \n    ret_bool = False\n         \n    #1,4\u6761\u5747\u7ebf\u591a\u5934\u6392\u5217\uff0c\u65e2\u5747\u7ebf1>2>3>4\n    #\u83b7\u5f97\u6628\u65e5\u7684MA\n    wk_MA1 = get_MA(data,MA1)\n    wk_MA2 = get_MA(data,MA2)\n    wk_MA3 = get_MA(data,MA3)\n    wk_MA4 = get_MA(data,MA4)\n    \n    if (wk_MA1[-1] > wk_MA2[-1]) & (wk_MA2[-1] > wk_MA3[-1]):\n        ret_bool = True\n         #print (\"Debug: MA high!\") \n    else:\n        ret_bool = False     \n\n    #print (\"Debug: MA selected!\")        \n    return ret_bool\n\n#\u5747\u7ebf\u7a7a\u5934\u6392\u5217\ndef determin_MA_Buy2(data):\n    \n    ret_bool = False\n    \n    #\u83b7\u5f97\u4ef7\u683c\n    wk_closePrice = data['closePrice']\n    wk_openPrice = data['openPrice']    \n         \n    #\u83b7\u5f97\u6628\u65e5\u7684MA\n    wk_MA1 = get_MA(data,MA1)\n    wk_MA2 = get_MA(data,MA2)\n    wk_MA3 = get_MA(data,MA3)\n    wk_MA4 = get_MA(data,MA4)\n\n    if (wk_MA1[-1] < wk_MA2[-1]) & (wk_MA2[-1] < wk_MA3[-1]) & (wk_MA3[-1] < wk_MA4[-1]):\n        ret_bool = True\n        #print (\"Debug: MA high!\") \n            \n    else:\n        ret_bool = False     \n\n    return ret_bool\n\n#\u9634\u7ebf\ndef determin_Down(data):\n    \n    ret_bool = False\n    \n    #\u83b7\u5f97\u4ef7\u683c\n    wk_closePrice = data['closePrice']\n    wk_openPrice = data['openPrice']\n    \n    if (wk_closePrice[-1] < wk_openPrice[-1]):\n        ret_bool = True\n         #print (\"Debug: MA high!\") \n    else:\n        ret_bool = False     \n\n    return ret_bool\n\n#\u9633\u7ebf\ndef determin_Up(data):\n    \n    ret_bool = False\n    \n    #\u83b7\u5f97\u4ef7\u683c\n    wk_closePrice = data['closePrice']\n    wk_openPrice = data['openPrice']\n    \n    if (wk_closePrice[-1] > wk_openPrice[-1]):\n        ret_bool = True\n         #print (\"Debug: MA high!\") \n    else:\n        ret_bool = False     \n\n    return ret_bool\n\n#\u5728\u5747\u7ebf\u4e0a\u65b9\ndef determin_Above_MA(data):\n    \n    ret_bool = False\n    \n    #\u83b7\u5f97\u4ef7\u683c\n    wk_closePrice = data['closePrice']\n\n    #\u83b7\u5f97\u6628\u65e5\u7684MA\n    wk_MA1 = get_MA(data,MA1)    \n    \n    if (wk_closePrice[-1] > wk_MA1[-1]):\n        ret_bool = True\n         #print (\"Debug: MA high!\") \n    else:\n        ret_bool = False     \n\n    return ret_bool\n\n#\u6628\u65e5\u4ef7\u683c\u4e0a\u7a7f\u5e03\u6797\u7ebf\u4e0a\u7ebf\uff0c\u6628\u65e5\u6536\u76d8\u4ef7\u683c\u5927\u4e8e\u5e03\u6797\u7ebf\u4e0a\u7ebf\n#\u51b3\u5b9a\u5e03\u6797\u7ebf\u4e70\u5165\u6761\u4ef6\ndef determin_BANDS_Buy(data):\n    \n    ret_bool = False\n    \n    #\u83b7\u5f97\u4ef7\u683c\n    wk_closePrice = data['closePrice']\n    wk_openPrice = data['openPrice']\n    \n    #\u83b7\u5f97\u5e03\u6797\u7ebf\n    wk_upper, wk_middle, wk_lower = get_BANDS(data)\n    \n    #\u6628\u65e5\u4ef7\u683c\u4e0a\u7a7f\u5e03\u6797\u7ebf\u4e0a\u7ebf\uff0c\u6628\u65e5\u6536\u76d8\u4ef7\u683c\u5927\u4e8e\u5e03\u6797\u7ebf\u4e0a\u7ebf\n    if (wk_closePrice[-1]>wk_upper[-1]) & (wk_upper[-1]>wk_openPrice[-1]) & (wk_closePrice[-2] < wk_upper[-2]):\n        ret_bool = True\n         #print (\"Debug: Bollinger buy!\") \n    else:\n        ret_bool = False     \n\n    #print (\"Debug: Bollinger Buy selected!\")        \n    return ret_bool\n\n#N\u65e5\u6700\u4f4e\u4ef7\uff0cN\u65e5\u66fe\u7ecf\u89e6\u53ca\u5e03\u6797\u4e0b\u8f68\uff0cN\u65e5\u6ca1\u6709\u89e6\u53ca\u5e03\u6797\u4e2d\u7ebf\ndef determin_BANDS_Buy2(data):\n    \n    N1 = BOLL_CHECK_DAYS\n    \n    ret_bool = False\n    wk_close = data['closePrice']  \n    wk_high = data['highPrice']\n    wk_low = data['lowPrice']\n    lastLow = wk_low[-1]\n    wk_low = wk_low[0:len(wk_low)-1] #\u53bb\u9664\u6628\u5929\u7684\u4ef7\u683c\n    wk_low = wk_low[len(wk_low)-N1:len(wk_low)] #\u83b7\u5f97N1\u5929\u7684\u6700\u4f4e\u4ef7\n    wk_low.sort() #\u6309\u7167\u5347\u5e8f\u6392\u5e8f\n    \n    #\u83b7\u5f97\u5e03\u6797\u7ebf\n    wk_upper, wk_middle, wk_lower = get_BANDS(data)    \n    \n    #\u6628\u65e5\u6700\u4f4e\u4ef7\u662f\u5426\u662f\u8fd1\u65e5\u6700\u4f4e\u4ef7\uff1f\n    #if lastLow >=  wk_low[0]*(1+NDAY_PERCENT_P):\n    #    return False\n    #else:\n    #    ret_bool = True\n    \n    #wk_high = wk_high[len(wk_high)-N1:len(wk_high)] #\u83b7\u5f97N1\u5929\u7684\n    #\u8fde\u7eedN\u65e5\u5728\u5e03\u6797\u4e2d\u8f68\uff1f\u662f,\u8fd4\u56detrue\n    #for index in range(1,len(wk_high)):\n    #    if wk_high[-index] > wk_upper[-index]:\n    #        return False\n    #        break\n    #    else:\n    #        ret_bool = True\n            \n    wk_low = data['lowPrice']        \n    wk_low = wk_low[len(wk_low)-N1:len(wk_low)] #\u83b7\u5f97N1\u5929\u7684\n    #N\u65e5\u5185\u662f\u5426\u89e6\u53ca\u5e03\u6797\u4e0b\u8f68\uff1f\u662f\uff0c\u8fd4\u56detrue\n    for index in range(1,len(wk_low)):\n        if wk_low[-index] < wk_lower[-index]:\n            #print \"Touch Boll Low!\"\n            return True\n            break\n        else:\n            ret_bool = False\n            \n    return ret_bool\n\n#\u5224\u65ad\u793e\u4ea4\u6570\u636e\u4e70\u5165\u6761\u4ef6\ndef determin_Social_Buy(stock,yesterday):\n    \n    ret_bool = False\n    \n    all_change = Get_SocialChange(stock,yesterday)\n    \n    if all_change >= SOCIAL_BUY_RATE :\n        ret_bool = True\n    \n    return ret_bool,all_change\n\n#\u51b3\u5b9a\u5356\u51fa\u6761\u4ef6\n# 1\uff0c\u5f53\u6700\u5c0f\u5747\u7ebf\u56de\u843d\u65f6\u3002\u6700\u5927\u6536\u76ca\u56de\u843d%M, \u6b62\u76c8\u3002\n# 2\uff0c\u5f53\u6700\u5c0f\u5747\u7ebf\u56de\u843d\u65f6\u3002\u76c8\u5229\u5230\u8fbe%P\uff0c\u6b62\u76c8\u3002\n# 3\uff0c\u4e8f\u635f\u5927\u4e8e%L, \u6b62\u635f\u3002\n# 4\uff0c\u5e03\u6797\u7ebf4\u5468\u671f\u6700\u5927\ndef determin_Sell(data,stock,context,account,valid_cash):\n    \n    #\u83b7\u5f97\u6700\u5c0f\u5747\u7ebf\n    wk_MA1 = get_MA(data,MA1)\n    wk_upper, wk_middle, wk_lower = get_BANDS(data)\n    \n    wk_position = account.get_position(stock)\n    if not wk_position:\n        return valid_cash\n    \n    #\u5f53\u6700\u5c0f\u5747\u7ebf\u56de\u843d\u65f6\n    if(wk_MA1[-1] < wk_MA1[-2]):\n        #\u76c8\u5229\u5230\u8fbe%P        \n        wk_profit_percent = wk_position.profit/(wk_position.value - wk_position.profit)\n        if (wk_profit_percent > TAKEPROFIT) & (wk_position.profit > 0):\n            account.order_to(stock,0)\n            valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n            log.info(\"Sell \"+str(stock)+\" by take profit at profit \"+str(wk_position.profit))\n            return valid_cash\n    if( wk_middle[-2] > wk_middle[-1]):\n        #\u6700\u5927\u6536\u76ca\u56de\u843d%M\n        wk_HighSinceBuy = context.hold_stock_info[context.hold_stock_info.index==stock].HighSinceBuy.tolist()[0]\n        wk_close = data['closePrice'][-1]\n        wk_open = data['openPrice'][-1]\n        wk_dif = (wk_close - wk_HighSinceBuy)/wk_HighSinceBuy\n        if (wk_dif<-DAYNAMIC_TP) & (wk_position.profit > 0) & (wk_close > wk_open):\n            account.order_to(stock,0)\n            valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n            log.info(\"Sell \"+str(stock)+\" by max down at profit \"+str(wk_position.profit))\n            return valid_cash      \n    \n    #\u5f53\u4ef7\u683c\u79bb\u5f00\u5e03\u6797\u4e0a\u7ebf\u65f6\n    #wk_high = data['highPrice']\n    #if( wk_upper[-2] < wk_high[-2]) & (wk_upper[-1] > wk_high[-1]):\n    #    account.order_to(stock,0)\n    #    valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n    #    log.info(\"Sell \"+str(stock)+\" by Bollinger high at profit \"+str(wk_position.profit))\n    #    return valid_cash        \n    \n    #\u4e8f\u635f\u5927\u4e8e%L, \u6b62\u635f\n    wk_profit_percent = wk_position.profit/(wk_position.value - wk_position.profit)\n    if (wk_profit_percent < -STOPLOSS) & (wk_position.profit < 0):\n        account.order_to(stock,0)\n        valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n        log.info(\"Sell \"+str(stock)+\" by stop loss at loss \"+str(wk_position.profit))\n        return valid_cash\n    \n    # \u5e03\u6797\u7ebf2\u6700\u5927,\u6b62\u76c8\n    #wk_closePrice = data['closePrice']\n    #wk_openPrice = data['openPrice']\n    #wk_upper2, wk_middle2, wk_lower2 = get_BANDS2(data)\n    #if (wk_closePrice[-1]>wk_upper2[-1]) & (wk_closePrice[-1] != wk_openPrice[-1]):\n    #    account.order_to(stock,0)\n    #    valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n    #    log.info(\"Sell \"+str(stock)+\" by Bands2 TP at profit \"+str(wk_position.profit))\n    #    return valid_cash\n    \n    #\u5e03\u6797\u7ebf\n    #wk_upper, wk_middle, wk_lower = get_BANDS(data)\n    #if (wk_middle[-2]>wk_middle[-1]) & (wk_position.profit > 0):\n    #    account.order_to(stock,0)\n    #    valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n    #    log.info(\"Sell \"+str(stock)+\" by Bands MID TP at profit \"+str(wk_position.profit))\n    #    return valid_cash    \n    \n    #Vol\u56de\u843d\uff0c\u6b62\u76c8\n    #\u83b7\u5f97\u6210\u4ea4\u91cf\u5747\u7ebf\n    #wk_Vol_MA1 = get_Vol_MA(data,MA1)\n    #wk_Vol_MA2 = get_Vol_MA(data,MA2)\n    #if (wk_Vol_MA1[-1] < wk_Vol_MA2[-1]) & (wk_Vol_MA1[-2] >= wk_Vol_MA2[-2]) & (wk_position.profit > 0):\n    #    account.order_to(stock,0)\n    #    valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n    #    log.info(\"Sell \"+str(stock)+\" by Vol Down TP at profit \"+str(wk_position.profit))\n    #    return valid_cash\n    \n    #\u5747\u7ebf\u4e0b\u65b9\n    #\u83b7\u5f97\u6628\u65e5\u7684MA\n    #wk_MA1 = get_MA(data,MA1)    \n    #if (wk_MA1[-1] > wk_closePrice[-1]) & (wk_position.profit > 0):\n    #    account.order_to(stock,0)\n    #    valid_cash = valid_cash + (context.current_price(stock)*wk_position.available_amount)\n    #    log.info(\"Sell \"+str(stock)+\" by MA1 TP at profit \"+str(wk_position.profit))\n    #    return valid_cash       \n        \n    return valid_cash\n\n#\u83b7\u5f97\u793e\u4ea4\u70ed\u5ea6\u53d8\u5316\ndef Get_SocialChange(stock,yesterday):\n    \n    all_change = 0\n    dayBeforeYesterday = cal.advanceDate(yesterday, '-1B', BizDayConvention.Following)            \n    XQ_data1 = DataAPI.SocialDataXQGet(ticker=stock[0:6],beginDate=yesterday,endDate=yesterday,field=u\"\",pandas=\"1\")\n    XQ_data2 = DataAPI.SocialDataXQGet(ticker=stock[0:6],beginDate=dayBeforeYesterday,endDate=dayBeforeYesterday,field=u\"\",pandas=\"1\")  \n    XQ_data1['Diff'] = XQ_data1['postPercent'] - XQ_data2['postPercent']\n\n    Guba_data1 = DataAPI.SocialDataGubaGet(ticker=stock[0:6],beginDate=yesterday,endDate=yesterday,field=u\"\",pandas=\"1\")\n    Guba_data2 = DataAPI.SocialDataGubaGet(ticker=stock[0:6],beginDate=dayBeforeYesterday,endDate=dayBeforeYesterday,field=u\"\",pandas=\"1\")  \n    Guba_data1['Diff'] =  Guba_data1['postPercent'] - Guba_data2['postPercent']\n\n    XQ_change = XQ_data1['Diff'].tolist()[0]\n    Guba_change = Guba_data1['Diff'].tolist()[0]\n    all_change = XQ_change + Guba_change\n        \n    return all_change\n\ndef initialize(context):\n    #context.hold_stock_info = pd.read_csv('BANDS_status.csv',encoding='GB18030')\n    #context.hold_stock_info = context.hold_stock_info.loc[:,['Date','Reason','NDaysPercent','HighSinceBuy'] ]\n    context.last_whole_value = 0\n    pass\n\ndef handle_data(context):\n    \n    account = context.get_account('fantasy_account')\n    \n    #reset hold_stock_info\n    context.hold_stock_info = pd.read_csv('BANDS_status_work_1468.csv',encoding='GB18030')\n    context.hold_stock_info = context.hold_stock_info.loc[:,['Stock','Date','Reason','NDaysPercent','HighSinceBuy','Vol','ROE'] ]\n    context.hold_stock_info = context.hold_stock_info.set_index('Stock')\n    \n    #reset last_whole_value\n    context.last_whole_value = pd.read_csv('portfolio_value_1468.csv')\n    context.last_whole_value = context.last_whole_value.loc[:,['Value'] ]\n    context.last_whole_value = context.last_whole_value['Value'].tolist()[0]    \n    \n    \n    log.info(\"START! \"+str(context.current_date))\n\n    #\u6628\u5929\u65e5\u671f\n    yesterday = context.previous_date\n    \n    #\u5b9a\u4e49\u53ef\u7528\u8d44\u91d1\u548c\u6bcf\u80a1\u4e70\u5165\u8d44\u91d1\n    valid_cash = account.cash\n    money = account.portfolio_value / stk_num\n    \n    #\u53ef\u4ea4\u6613\u80a1\u7968(\u53bb\u9664\u505c\u724c)\n    valid_universe = context.get_universe(exclude_halt=True)\n    \n    #\u83b7\u5f97\u6301\u4ed3\n    positions = account.get_positions()\n    \n    #print context.hold_stock_info\n    #\u540c\u6b65context.hold_stock_info\n    for stock in context.hold_stock_info.index:\n        if stock not in positions:\n            context.hold_stock_info = context.hold_stock_info.drop(stock)\n        \n    #\u6e05\u6d17\u80a1\u7968\u5217\u8868\n    univ = valid_universe    \n    #\u53bb\u9664\u6301\u4ed3\u80a1\u7968\n    univ = [s for s in univ if s not in positions]\n    \n    #\u53bb\u9664\u505c\u724c\u80a1\u7968        \n    univ_to_buy = pd.DataFrame(columns=['Date','Reason','NDaysPercent','HighSinceBuy','YINZI'])\n    \n    #\u66f4\u65b0\u6570\u636e\n    for stock in context.hold_stock_info.index:\n        #\u8bbe\u7f6e\u6700\u5927close\n        wk_high = context.history(stock, \"highPrice\", 1, freq='1d', style='sat')\n        wk_high = wk_high[stock]['highPrice'][0]\n        wk_HighSinceBuy = context.hold_stock_info[context.hold_stock_info.index==stock].HighSinceBuy.tolist()[0]\n        if wk_high > wk_HighSinceBuy:\n            context.hold_stock_info.set_value(stock,'HighSinceBuy',wk_high)\n            \n    #\u5224\u65ad\u5356\u51fa\n    for stock in positions:        \n        \n        #\u83b7\u5f97\u6240\u6709\u5386\u53f2\u6570\u636e\n        data,histResult = GetHistoryDate(stock,MAX_HISTORY_DAYS,account,context)\n        \n        #\u6570\u636e\u5f02\u5e38\uff0c\u4e0d\u7ee7\u7eed\u8fdb\u884c\n        if histResult == False:\n            continue\n        ##\u5356\u51fa        \n        valid_cash = determin_Sell(data,stock,context,account,valid_cash)\n            \n    #\u5faa\u73af\u6240\u6709\u80a1\u7968\n    for stock in univ:\n        \n        #print stock\n        #\u83b7\u5f97\u6240\u6709\u5386\u53f2\u6570\u636e\n        data,histResult = GetHistoryDate(stock,MAX_HISTORY_DAYS,account,context)\n        #\u6570\u636e\u5f02\u5e38\uff0c\u4e0d\u7ee7\u7eed\u8fdb\u884c\n        if histResult == False:\n            continue\n        #\u5220\u9664\u6628\u65e5\u6da8\u505c\n        wk_closePrice = data['closePrice']\n        wk_openPrice = data['openPrice']\n        if (((wk_closePrice[-1]-wk_closePrice[-2])/wk_closePrice[-2])>0.099):\n            continue\n        #\u5220\u9664\u6628\u65e5\u590d\u76d8\n        wk_open = context.history(stock, \"openPrice\", 2, freq='1d', style='sat')\n        wk_open = wk_open[stock]['openPrice'][-2]        \n        if wk_open == 0:\n            continue\n                    \n        #\u5224\u65ad\u4e70\u5165\u6761\u4ef6\n        #wk_Vol = determin_Vol_Buy(data)\n        #wk_Up = determin_Up(data)\n        #wk_MA2 = determin_MA_Buy2(data)\n        #wk_Down = determin_Down(data)\n        #wk_NDay_Min = determin_NDay_Min(data)\n        wk_MA = determin_MA_Buy(data)\n        #wk_Above_MA = determin_Above_MA(data)\n        #wk_BANDS = determin_BANDS_Buy(data) \n        #wk_BANDS2 = determin_BANDS_Buy2(data)\n        wk_Social,all_change = determin_Social_Buy(stock,yesterday)\n        if (wk_Social == True) & (wk_MA == True):\n            #\u8bbe\u5b9a\u65e5\u671f\n            univ_to_buy.set_value(stock,'Date',context.previous_date)\n            ##\u4e70\u5165\u7406\u7531\n            univ_to_buy.set_value(stock,'Reason','BANDS') \n            #N\u65e5Percent\n            wk_percent = get_UPDOWN_Percent(data)\n            if wk_percent > 0.10:\n                univ_to_buy = univ_to_buy.drop(stock)\n                continue\n            else:\n                univ_to_buy.set_value(stock,'NDaysPercent',wk_percent)     \n            #\u4e70\u5165\u540e\u6700\u5927close\n            univ_to_buy.set_value(stock,'HighSinceBuy',data['highPrice'][-1])\n            #Name\n            wk_Name = DataAPI.EquGet(equTypeCD=u\"A\",secID=stock,field=u\"secShortName\",pandas=\"1\")\n            univ_to_buy.set_value(stock,'Name',wk_Name.secShortName.tolist()[0])  \n            #\u793e\u4ea4\u6307\u6570\n            univ_to_buy.set_value(stock,'YINZI',all_change)\n\n    #\u6392\u5e8f\u6240\u6709\u9009\u51fa\u7684\u80a1\u7968\n    if context.last_whole_value > account.reference_portfolio_value