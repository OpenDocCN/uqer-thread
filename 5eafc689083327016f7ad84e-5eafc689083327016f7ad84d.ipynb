'fantasy_account')\n    universe = context.get_universe()\n    context.short_t = 20\n    context.long_t = 60\n    context.windows = 61\n    \n    # \u83b7\u53d6\u56e0\u5b50PE\u7684\u7684\u5386\u53f2\u6570\u636e\u96c6\uff0c\u622a\u6b62\u5230\u524d\u4e00\u4e2a\u4ea4\u6613\u65e5\n    hist = context.history(universe, attribute='closePrice', time_range=context.windows, style='sat')\n    log.info(hist)\n    close_price = np.array(hist[universe[0]]['closePrice'])\n    short_ma = ta.MA(close_price, context.short_t)\n    long_ma = ta.MA(close_price, context.long_t)\n    log.info(short_ma)\n    \n    position = account.get_position(universe[0])\n    \n    if position:\n        if (short_ma[-2]>long_ma[-2]) and (short_ma[-1]<long_ma[-1]):      # and \u4e24\u8fb9\u7684\u6761\u4ef6\u90fd\u9700\u8981\u52a0\u4e0a\u62ec\u53f7\uff0c \u4e0d\u52a0\u4e0a\u62ec\u53f7\u7684\u8bdd\u7ed3\u679c\u4f1a\u4e0d\u4e00\u6837   \uff01\uff01\uff01\n            account.order_pct_to(universe[0], 0)\n            \n    if not position:\n        if (short_ma[-2]<long_ma[-2]) and (short_ma[-1]>long_ma[-1]):\n            account.order_pct(universe[0], 1)\n     \n", "language": "python", "metadata": {}, "outputs": [{"ename": "ImportError", "evalue": "cannot import name api_base", "output_type": "pyerr", "traceback": ["\u001b[1;31m---------------------------------------------------------------------------\u001b[0m\n\u001b[1;31mImportError\u001b[0m                               Traceback (most recent call last)", "\u001b[1;32m<mercury-input-4-486728A91ECA4B31A1A8A94EE053C48C>\u001b[0m in \u001b[0;36m<module>\u001b[1;34m()\u001b[0m\n\u001b[1;32m----> 1\u001b[1;33m \u001b[1;32mfrom\u001b[0m \u001b[0mquartz\u001b[0m\u001b[1;33m.\u001b[0m\u001b[0mapi\u001b[0m \u001b[1;32mimport\u001b[0m \u001b[1;33m*\u001b[0m\u001b[1;33m;\u001b[0m\u001b[1;32mfrom\u001b[0m \u001b[0mquartz\u001b[0m\u001b[1;33m.\u001b[0m\u001b[0mtrade\u001b[0m\u001b[1;33m.\u001b[0m\u001b[0mcost\u001b[0m \u001b[1;32mimport\u001b[0m \u001b[0mSlippage\