{"metadata": {"signature": "sha256:34283a54e9763c434d36b809e36001e025c197925e82e550cdffde4c8b4c2b23"}, "nbformat": 3, "nbformat_minor": 0, "worksheets": [{"cells": [{"cell_type": "markdown", "id": "4530E98A7F7441E395B1627016C2950D", "metadata": {}, "source": "# "}, {"cell_type": "strategy", "collapsed": false, "has_detail": true, "id": "uqer_939FD115E5B847998", "input": "import math\nimport numpy as np\nimport pandas as pd\nfrom sklearn import preprocessing, svm\nimport matplotlib.pyplot as plt\nfrom matplotlib import style\nimport sklearn\nimport time\nfrom datetime import datetime\nimport cPickle as pickle\nfrom CAL.PyCAL import *\n\nstart = '2014-01-01'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2014-03-30'                         # \u56de\u6d4b\u7ed3\u675f\u65f6\u95f4\nbenchmark = 'HS300'                        # \u7b56\u7565\u53c2\u8003\u6807\u51c6\nuniverse = set_universe('A')  # \u8bc1\u5238\u6c60\uff0c\u652f\u6301\u80a1\u7968\u548c\u57fa\u91d1\ncapital_base = 1000000                      # \u8d77\u59cb\u8d44\u91d1\nfreq = 'd'                                 # \u7b56\u7565\u7c7b\u578b\uff0c'd'\u8868\u793a\u65e5\u95f4\u7b56\u7565\u4f7f\u7528\u65e5\u7ebf\u56de\u6d4b\uff0c'm'\u8868\u793a\u65e5\u5185\u7b56\u7565\u4f7f\u7528\u5206\u949f\u7ebf\u56de\u6d4b\nrefresh_rate = 1\n\ndef initialize(account):\n    account.train=True\n    #global train\n\ndef SaveVarToFile(Var,filename):#\u4fdd\u5b58\u53d8\u91cfVar\u5230\u6587\u4ef6file \u4e00\u4e2a\u6587\u4ef6\u6682\u65f6\u53ea\u80fd\u4fdd\u5b58\u4e00\u4e2aVar \u6587\u4ef6\u540e\u7f00\u4e3aTXT\n    import cPickle as pickle   \n    write(filename,pickle.dumps(Var), encoding = 'utf-8', mode = 'w')\n    return filename\n\ndef LoadVarFromFile(filename):#\u4ece\u6587\u4ef6file\u8bfb\u53d6var  \u4e00\u4e2a\u6587\u4ef6\u6682\u65f6\u53ea\u80fd\u4fdd\u5b58\u4e00\u4e2aVar\n    import cPickle as pickle \n    p2=str(read(filename,encoding = 'utf-8'))\n    Var = pickle.loads(p2)  \n    return Var    \n\ndef train_data(year_date, index_date):\n    Valuation=[]\n    price=[]\n    status=[]\n    \n    SZ1=DataAPI.IdxConsGet(secID=u\"399008.ZICN\",intoDate=index_date,field=u\"consID\",pandas=\"1\")\n    SZ2=DataAPI.IdxConsGet(secID=u\"399012.ZICN\",intoDate=index_date,field=u\"consID\",pandas=\"1\")\n    SH=DataAPI.IdxConsGet(secID=u\"399905.ZICN\",intoDate=index_date,field=u\"consID\",pandas=\"1\")\n    \n    tem_index=SZ1.append(SZ2).append(SH)\n    unix_30d=60*60*24*30\n    unix_weekend=60*60*24*3\n    \n    incm=DataAPI.FdmtISGet(secID=tem_index['consID'],endDate=year_date,beginDate=year_date,\n                           endDateRep=year_date,beginDateRep=year_date,\n                           field=('secID','publishDate','operateProfit','NoperateIncome','NoperateExp',\n                           'TProfit','NIncome','NIncomeAttrP','basicEPS'), pandas=\"1\")\n    \n    date=incm['publishDate']\n    index=incm['secID']\n    \n    year_past=str(int(year_date)-10000)\n    incm_past= DataAPI.FdmtISGet(secID=tem_index['consID'],reportType=u\"Q1\",endDate=year_past,beginDate=year_past,\n               endDateRep=year_past,beginDateRep=year_past,\n               field=('secID','publishDate','operateProfit','NoperateIncome','NoperateExp',\n               'TProfit','NIncome','NIncomeAttrP','basicEPS'), pandas=\"1\")\n    \n    del(incm['secID'],incm['publishDate'],incm_past['secID'],incm_past['publishDate'])\n    #future30day=cal.advanceDate(account.previous_date,'30B')\n    \n    for each in range(0,len(date)):\n        \n        indicator=DataAPI.MktStockFactorsOneDayGet(tradeDate=year_date,secID=index[each],\n          field=('pe,PB,ROA,ROE,ASSI,CTOP,CurrentAssetsRatio,CurrentAssetsTRate,CurrentRatio,'\n          'DebtsAssetRatio,EPS,EGRO,EquityToAsset,FinancialExpenseRate,GrossIncomeRatio,'\n          'NetAssetGrowRate,NetProfitGrowRate,NPParentCompanyGrowRate,OperatingProfitRatio'),pandas=\"1\")\n        \n        date_stamp = datetime.strptime(date[each], '%Y-%m-%d')\n        unix=time.mktime(date_stamp.timetuple())\n        unix_30_late=unix+unix_30d\n        \n        Valuation.append(indicator.iloc[0].tolist())\n        \n        p1=DataAPI.MktEqudAdjGet(tradeDate=date[each],secID=index[each],field=u\"closePrice\",pandas=\"1\")\n        #print p1\n        if not p1.empty:\n            pass\n        else:\n            p1_weekend=datetime.fromtimestamp(unix-unix_weekend).strftime('%Y-%m-%d')\n            p1=DataAPI.MktEqudAdjGet(tradeDate=p1_weekend,secID=index[each],field=u\"closePrice\",pandas=\"1\")\n            \n        p1_30d=datetime.fromtimestamp(unix_30_late).strftime('%Y-%m-%d')\n        p2=DataAPI.MktEqudAdjGet(tradeDate=p1_30d,secID=index[each],field=u\"closePrice\",pandas=\"1\")\n        \n        if not p2.empty:\n            pass\n        else:\n            date_stamp2 = datetime.strptime(p1_30d, '%Y-%m-%d')\n            unix2=time.mktime(date_stamp2.timetuple())\n            unix2_weekend=unix2-unix_weekend\n            p2_weekend=datetime.fromtimestamp(unix2_weekend).strftime('%Y-%m-%d')\n            p2=DataAPI.MktEqudAdjGet(tradeDate=p2_weekend,secID=index[each],field=u\"closePrice\",pandas=\"1\")\n\n        dif =  p2.values / p1.values\n\n        if dif > 1.1:\n            s=1\n        else:\n            s=0\n        status.append(s)\n        \n        price.append(p1.iloc[0].tolist())\n            \n    Valuation=pd.DataFrame(Valuation, columns=['pe','PB','ROA','ROE','ASSI','CTOP','CurrentAssetsRatio','CurrentAssetsTRate','CurrentRatio',\n          'DebtsAssetRatio','EPS','EGRO','EquityToAsset','FinancialExpenseRate','GrossIncomeRatio',\n          'NetAssetGrowRate','NetProfitGrowRate','NPParentCompanyGrowRate','OperatingProfitRatio'])\n    price=pd.DataFrame(price, columns=['price'])\n    status=pd.DataFrame(status, columns=['status'])\n    df=pd.concat([incm,Valuation,price,status], axis=1)\n    \n    return df\n\ndef fundamental(t,t_d,index):\n    Valuation=[]\n    price=[]\n    status=[]\n    \n    incm=DataAPI.FdmtISGet(secID=index,endDate=t,beginDate=t,\n                           endDateRep=t,beginDateRep=t,\n                           field=('secID','publishDate','operateProfit','NoperateIncome','NoperateExp',\n                           'TProfit','NIncome','NIncomeAttrP','basicEPS'), pandas=\"1\")\n    \n    index2=incm['secID']\n\n    del(incm['secID'],incm['publishDate'])\n    \n    Valuation=DataAPI.MktStockFactorsOneDayGet(tradeDate=t,secID=index,\n          field=('pe,PB,ROA,ROE,ASSI,CTOP,CurrentAssetsRatio,CurrentAssetsTRate,CurrentRatio,'\n          'DebtsAssetRatio,EPS,EGRO,EquityToAsset,FinancialExpenseRate,GrossIncomeRatio,'\n          'NetAssetGrowRate,NetProfitGrowRate,NPParentCompanyGrowRate,OperatingProfitRatio'),pandas=\"1\")\n    \n    price=DataAPI.MktEqudAdjGet(tradeDate=t_d,secID=list(index2),field=u\"closePrice\",pandas=\"1\")\n\n    price=pd.DataFrame(price, columns=['price'])\n    df=pd.concat([incm,Valuation,price], axis=1)\n    #print df\n    X=np.array(df.replace('NaN', 999).replace('',999).values.tolist())\n\n    X=preprocessing.scale(X)\n    return X, index2\n\ndef handle_data(account):\n    if account.train:\n        index_date=str('20140301')\n        df1=train_data(str('20140331'),index_date)\n        df2=train_data(str('20140630'),index_date)\n        df3=train_data(str('20140930'),index_date)\n        df4=train_data(str('20141231'),index_date)\n        df=pd.concat([df1,df2,df3,df4], axis=0)\n        df.iloc[np.random.permutation(len(df))]\n        y=df['status'].values.tolist()\n        del(df['status'])\n        print(\"<===== shape of training dataset \", str(df.shape))\n        X=np.array(df.replace('NaN', 999).values.tolist())\n\n        X=preprocessing.scale(X)\n        \n        clf = svm.SVC(kernel=str(\"linear\"), C=1.0)\n        clf.fit(X, y)\n        \n        SaveVarToFile(clf,'Indexlist.txt')#\u4fdd\u5b58\u6587\u4ef6\u91cc\uff0c\u5c31\u53ef\u4ee5\u4e0b\u8f7d\u5230\u7535\u8111\u4f7f\u7528\u4e86\n        account.train=False\n    \n    \n    clf = LoadVarFromFile('Indexlist.txt')\n    \n    td=str(account.current_date).replace('-','')[:8]\n    \n    if int(td[4:]) > 930 and int(td[4:])< 1232:\n        t=str(td[:4])+'0930'\n    elif int(td[4:])> 630 and int(td[4:])< 931:\n        t=str(td[:4])+'0630'\n    elif int(td[4:])> 331 and int(td[4:])< 631:\n        t=str(td[:4])+'0331'\n    else:\n        t=str(int(td[:4])-1)+'1231'\n    \n    SZ1=DataAPI.IdxConsGet(secID=u\"399008.ZICN\",intoDate=td,field=u\"consID\",pandas=\"1\")\n    SZ2=DataAPI.IdxConsGet(secID=u\"399012.ZICN\",intoDate=td,field=u\"consID\",pandas=\"1\")\n    SH=DataAPI.IdxConsGet(secID=u\"399905.ZICN\",intoDate=td,field=u\"consID\",pandas=\"1\")\n    index=SZ1.append(SZ2).append(SH)\n    \n    X, index2=fundamental(t,td,index['consID'])\n\n    for each in range(0, len(index2)):\n        #print clf.predict(X[each].reshape(1,X.shape[1]))[0]\n        if clf.predict(X[each].reshape(1,X.shape[1]))[0] == 1 and index2[each] not in account.security_position.keys():\n            print(\"===================Buying:\", index2[each])\n            order_pct(index2[each], 0.25)\n        if clf.predict(X[each].reshape(1,X.shape[1]))[0] == 0 and index2[each] in account.security_position.keys():\n            print(\"<<<<<<<<<<<<<<<<<<Holding:\", account.security_position.keys())\n            print(\"-------------------selling:\", index2[each])\n            order_to(index2[each], 0)\n        \n        \n        \n    if account.security_position.keys():\n        for stock in account.security_position.keys():\n            cur_price = data[stock].close\n            position=account.security_position[stock]\n            if cur_price > position.security_cost * (1 + 0.5) or cur_price < position.security_cost * (1 - 0.2):\n                order_target(stock, 0)\n                log.info(\"<<<<<<<<<<<\", stock, \"%s lose:\", 1-cur_price/position.security_cost)\n", "language": "python", "metadata": {}, "outputs": [{"output_type": "stream", "stream": "s