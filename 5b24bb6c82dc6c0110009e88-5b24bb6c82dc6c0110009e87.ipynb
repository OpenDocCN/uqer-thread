}\n    momentum2 = {'symbol':[], 'c_ret':[]}\n    for future in symbollist:\n        if history[future][0] and (history[future][-1]/history[future][0]>1):\n            momentum1['symbol'].append(future)\n            momentum1['c_ret'].append(history[future][-1]/history[future][0])\n        else:\n            momentum2['symbol'].append(future)\n            momentum2['c_ret'].append(history[future][-1]/history[future][0])\n            \n    buylist = momentum1['symbol']\n    selllist = momentum2['symbol']\n    \n    # \u5e73\u6389\u4e0d\u9700\u8981\u7684\u671f\u8d27\n    for symbol1 in symbollist:\n        if symbol1 not in buylist and symbol1 not in selllist:\n            close_all_positions(symbol1)\n\n    # \u4e70\u5165\u7b5b\u9009\u51fa\u7684\u671f\u8d27\n    for symbol2 in buylist:\n        if symbol2 not in symbollist:\n            futures_account.order(symbol2, context.amount, 'open')\n\n    # \u5356\u51fa\u7b5b\u9009\u51fa\u7684\u671f\u8d27\n    for symbol3 in selllist:\n        if symbol3 not in symbollist:\n            futures_account.order(symbol3, -context.amount, 'open')\n\ndef main_contract_mapping_changed(context, futures_account):\n    '''\n        \u5904\u7406\u79fb\u4ed3\u6362\u6708\u7684\u60c5\u51b5\n    '''\n    global current_universe\n    for symbol in current_universe:\n        if context.mapping_changed(symbol):\n            symbol_before, symbol_after = context.get_rolling_tuple(symbol)\n            if futures_account.get_position(symbol_before):\n                futures_account.switch_position(symbol_before, symbol_after)\n                return True\n        \n        return False\n", "language": "python", "metadata": {}, "outputs": [], "trading_days": "", "trusted": true}], "metadata": {}}]}